    "ingredients": [
        "2 pounds carrots, peeled and sliced",
        "\u00bc cup butter, melted",
        "\u00bc cup orange juice",
        "\u2153 cup brown sugar",
        "\u2153 cup white sugar",
        "1 teaspoon ground cinnamon"

    "ingredients": [
        "cooking spray",
        "1 cup brown sugar",
        "\u2154 cup sunflower seed spread (such as SunButter\u00ae)",
        "\u00bd cup butter, melted",
        "\u00bc cup light corn syrup",
        "1 cup bittersweet chocolate chips",
        "\u2153 cup raisins",
        "3 cups quick-cooking oats"
    ],
    "directions": [
        "Preheat the oven to 350 degrees F (175 degrees C)",
        "Spray a 9x13-inch baking pan with cooking spray",
        "Mix brown sugar, sunflower seed spread, butter, and corn syrup in a large bowl until combined",
        "Stir in chocolate chips and raisins",
        "Mix in oats",
        "Press mixture evenly into the prepared pan",
        "Bake in the preheated oven until golden brown, about 30 minutes",
        "Let cool for 1 hour before cutting"

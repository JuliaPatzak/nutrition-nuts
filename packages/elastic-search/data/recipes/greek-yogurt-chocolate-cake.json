    "ingredients": [
        "1\u2009\u00be cups self-rising flour",
        "1 cup unsweetened cocoa powder",
        "\u215b teaspoon salt",
        "2 cups white sugar",
        "1 cup water",
        "2  eggs",
        "\u00bc cup vanilla Greek yogurt",
        "2 teaspoons vanilla extract"
    ],
    "directions": [
        "Preheat the oven to 350 degrees F (175 degrees C)",
        "Sift flour, cocoa powder, and salt together in a bowl",
        "Mix sugar, water, eggs, Greek yogurt, and vanilla extract together in another bowl using an electric mixer",
        "Gradually add the flour-cocoa mixture into the bowl with the yogurt mixture",
        "Mix until well blended",
        "Pour batter into two 9-inch cake pans",
        "Bake in the preheated oven until a toothpick inserted into the centers comes out clean, 30 to 35 minutes"
    ],

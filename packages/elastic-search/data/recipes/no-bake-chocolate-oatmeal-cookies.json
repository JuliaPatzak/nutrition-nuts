    "ingredients": [
        "2 cups white sugar",
        "\u00bd cup unsweetened cocoa powder",
        "\u00bd cup butter",
        "\u00bd cup milk",
        "2 cups oatmeal",
        "1 cup flaked coconut",
        "1 teaspoon vanilla extract"
    ],
    "directions": [
        "Combine sugar, cocoa powder, butter, and milk in a saucepan over high heat and boil for 5 minutes",
        "Turn off heat and immediately stir in oatmeal, coconut, and vanilla extract",
        "Drop by tablespoonfuls onto wax paper and let cool in the refrigerator for 1 hour"
    ],

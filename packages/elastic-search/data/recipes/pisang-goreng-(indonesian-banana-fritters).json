    "ingredients": [
        "1\u2009\u00bc cups all-purpose flour",
        "2 tablespoons granulated sugar",
        "\u00bc tablespoon vanilla powder",
        "\u00bd cup milk",
        "1  egg",
        "2 tablespoons butter, melted",
        "1 teaspoon rum flavoring",
        "4  ripe bananas, sliced",
        "2 cups oil for frying"
    ],
    "directions": [
        "In a large bowl, combine flour, sugar and vanilla powder",
        "Make a well in the center, and pour in milk, egg, melted butter and rum flavoring",
        "Mix until smooth",
        "Fold in banana slices until evenly coated",
        "Heat oil in a wok or deep-fryer to 375 degrees F (190 degrees C)",
        "Drop banana mixture by tablespoon into hot oil",
        "Fry until golden brown and crispy, 10 to 15 minutes",
        "Remove bananas from oil, and drain on paper towels",
        "Serve hot"
    ],

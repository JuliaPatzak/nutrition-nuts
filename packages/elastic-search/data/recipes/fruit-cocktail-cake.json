    "ingredients": [
        "1 cup all-purpose flour",
        "1 cup white sugar",
        "1  egg",
        "1 teaspoon baking soda",
        "1 teaspoon vanilla extract",
        "1 (16 ounce) can fruit cocktail",
        "\u00bd cup packed brown sugar"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Lightly grease one 9x9 inch square baking pan",
        "Combine the flour, white sugar, egg, baking soda, vanilla and undrained fruit cocktail",
        "Mix until blended",
        "Pour batter into the prepared pan and sprinkle the top with the brown sugar",
        "Bake at 350 degrees F (175 degrees C) for 40 minutes or until golden brown and firm"
    ],

    "ingredients": [
        "3 tablespoons whole wheat flour",
        "1 tablespoon old-fashioned rolled oats",
        "\u00bc teaspoon baking soda",
        "\u00bc teaspoon ground cinnamon",
        "1 pinch ground cloves",
        "1 pinch ground nutmeg",
        "1 pinch baking powder",
        "1 pinch salt",
        "1  egg white",
        "1 tablespoon milk, or more as needed",
        "1 tablespoon brown sugar, or more to taste",
        "\u00bd teaspoon vanilla extract",
        "\u00bd cup fresh blueberries"
    ],
    "directions": [
        "Mix flour, oats, baking soda, cinnamon, cloves, nutmeg, baking powder, and salt together in a microwave-safe mug",
        "Stir egg white, milk, brown sugar, and vanilla extract into flour mixture until batter is fully combined; slowly stir in blueberries",
        "Cook in microwave for 1 minute",
        "Check muffin for doneness and continue cooking in 15 second increments until cooked through, about 1 minute more"
    ],

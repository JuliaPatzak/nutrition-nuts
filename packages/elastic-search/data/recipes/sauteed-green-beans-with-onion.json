    "ingredients": [
        "1 pound fresh green beans, trimmed",
        "1 pinch salt",
        "4 tablespoons extra-virgin olive oil",
        "1 medium onion, finely diced",
        "1  shallot, chopped",
        "\u00bd teaspoon garlic salt",
        "\u00bd teaspoon soy sauce",
        "salt and ground black pepper to taste"
    ],
    "directions": [
        "Fill a stockpot with water and bring to a boil over high heat",
        "Add green beans and 1 pinch salt; boil until tender, 5 to 10 minutes",
        "Drain beans in a colander and rinse with cold water",
        "Blot dry using paper towels",
        "Heat olive oil in a skillet over medium heat",
        "Cook and stir onion and shallot in the hot oil until tender and translucent, 5 to 7 minutes",
        "Add green beans, garlic salt, soy sauce, salt, and pepper",
        "Cook and stir for 5 to 7 minutes more",
        "Serve immediately"
    ],

    "ingredients": [
        "1 cup Martha White\u00ae Plain Enriched Yellow Corn Meal",
        "1 cup Pillsbury BEST\u00ae All Purpose Flour",
        "1 tablespoon baking powder",
        "\u00bd teaspoon ground cinnamon",
        "1  egg, lightly beaten",
        "\u00bd cup milk",
        "\u00bd cup honey",
        "\u2153 cup Crisco\u00ae Pure Corn Oil",
        "\u00bc cup sour cream"
    ],
    "directions": [
        "Heat oven to 400 degrees F",
        "Line 12 muffin cups with paper baking cups",
        "Combine corn meal, flour, baking powder and cinnamon in large bowl",
        "Whisk egg, milk, honey, oil and sour cream in small bowl",
        "Add to corn meal mixture, stirring just until moistened",
        "Spoon evenly into prepared muffin cups",
        "Bake 18 to 20 minutes or until lightly browned",
        "Cool 2 minutes",
        "Remove to wire rack to cool completely"
    ],

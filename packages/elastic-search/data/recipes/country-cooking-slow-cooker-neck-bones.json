    "ingredients": [
        "3 pounds pork neck bones",
        "1 small onion, chopped",
        "3 cloves garlic, minced, or more to taste",
        "1 teaspoon salt  (Optional)",
        "1 teaspoon dried thyme leaves",
        "1 tablespoon distilled white vinegar",
        "4 cups water"
    ],
    "directions": [
        "Place the neck bones into a slow cooker",
        "Sprinkle in the onion, garlic, salt, and thyme leaves",
        "Pour in the vinegar and water",
        "Cover and cook on High until the meat is tender, about 4 hours"

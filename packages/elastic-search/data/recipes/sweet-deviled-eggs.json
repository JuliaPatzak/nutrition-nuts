    "ingredients": [
        "6  large eggs",
        "ice cubes",
        "3 tablespoons mayonnaise",
        "1 tablespoon white sugar",
        "1 teaspoon honey mustard",
        "1 teaspoon vinegar",
        "\u00bd teaspoon salt",
        "\u00bd teaspoon ground black pepper",
        "1 pinch paprika, for garnish"
    ],
    "directions": [
        "Place eggs in a saucepan and cover with water",
        "Bring to a boil, remove from heat, and let eggs stand in hot water for 15 minutes",
        "Remove eggs from hot water and place in a bowl of ice water",
        "Peel",
        "Slice eggs in half lengthwise",
        "Remove yolks and transfer to a bowl; mash with a fork or pastry blender until yolks resemble powder",
        "Stir mayonnaise, sugar, honey mustard, vinegar, salt, and pepper into yolks until smooth",
        "Spoon yolk mixture into the egg whites and lightly sprinkle with paprika",
        "Chill before serving"
    ],

    "ingredients": [
        "2  tomatoes, diced",
        "1 cup shelled, cooked edamame",
        "1 cup frozen sweet corn kernels, thawed",
        "4  green onions, sliced",
        "2 tablespoons fresh lime juice",
        "2 tablespoons olive oil",
        "1 tablespoon chopped fresh cilantro",
        "2 cloves garlic, minced, or to taste",
        "1 pinch coarse sea salt to taste  (Optional)"
    ],
    "directions": [
        "Mix tomatoes, edamame, corn, green onions, lime juice, olive oil, cilantro, and garlic together in a bowl",
        "Season with sea salt",
        "Cover bowl with plastic wrap and refrigerate until flavors blend, at least 1 hour"

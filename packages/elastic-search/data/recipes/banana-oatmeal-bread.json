    "ingredients": [
        "\u00bd cup shortening",
        "1 cup white sugar",
        "2  eggs, beaten",
        "\u00bd teaspoon vanilla extract",
        "1 cup all-purpose flour",
        "1 cup quick cooking oats",
        "1 teaspoon baking soda",
        "\u00bd teaspoon salt",
        "\u00bc cup milk",
        "\u00bd teaspoon ground cinnamon",
        "1\u2009\u00bd cups mashed bananas",
        "\u00bd cup chopped raisins  (Optional)"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease a 9x5-inch loaf pan and set aside",
        "Cream together the shortening and sugar",
        "Add eggs and vanilla; beat until fluffy",
        "Sift together the flour, oatmeal, baking soda, salt and cinnamon",
        "Add dry ingredients alternately with bananas and milk",
        "Mix until blended",
        "Fold in raisins and pour into prepared pan",
        "Bake for 50 to 60 minutes; remove from oven and cover for 5 minutes"
    ],

    "ingredients": [
        "2 cups all-purpose flour",
        "\u00bd cup white sugar",
        "1\u2009\u00bd teaspoons baking powder",
        "\u00bd teaspoon baking soda",
        "\u00bd teaspoon salt",
        "1  egg",
        "\u00bc cup vegetable oil",
        "1 tablespoon orange zest",
        "2 tablespoons orange juice",
        "2 cups prepared mincemeat pie filling"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease a 9 x 5 x 3 inch loaf pan",
        "Mix together the egg, oil, orange peel, juice, and mincemeat, blending well",
        "Combine flour, sugar, baking powder, baking soda, and salt; add gradually to the egg mixture",
        "Mix just until moist",
        "Pour into prepared pan",
        "Bake for 1 hour",
        "Remove the loaf from the oven, and cool on a wire rack"
    ],

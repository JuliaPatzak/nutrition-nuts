    "ingredients": [
        "3 cups all-purpose flour",
        "2 cups white sugar",
        "2 teaspoons baking powder",
        "1 teaspoon baking soda",
        "1 teaspoon salt",
        "2  eggs, lightly beaten",
        "\u00be cup milk",
        "2 teaspoons vanilla extract",
        "1 cup melted butter, cooled",
        "2  bananas, mashed",
        "1  banana, chopped",
        "1 cup granola",
        "1 cup chopped walnuts",
        "1 cup shredded coconut",
        "\u00bc cup banana chips  (Optional)"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Line 24 muffin cups with paper liners",
        "In a bowl, mix the flour, sugar, baking powder, baking soda, and salt",
        "Mix in the eggs, milk, vanilla, and butter",
        "Fold in mashed bananas, chopped banana, granola, walnuts, and coconut",
        "Scoop into the prepared muffin cups",
        "Sprinkle with banana chips",
        "Bake 25 minutes in the preheated oven, or until a knife inserted in the center of a muffin comes out clean"
    ],

    "ingredients": [
        "1 tablespoon butter",
        "2 tablespoons water",
        "2 tablespoons all-purpose flour",
        "1 tablespoon non-fat dry milk",
        "\u215b teaspoon baking powder",
        "\u215b teaspoon ground cinnamon",
        "1 pinch salt",
        "1 (4 ounce) container sliced peaches, well drained"
    ],
    "directions": [
        "Melt butter in a mug in microwave, about 20 seconds",
        "Stir water, flour, dry milk, baking powder, ground cinnamon, and salt into butter until well blended",
        "Place peach slices on top of batter",
        "Microwave on 70% power until cooked through, about 2 minutes",
        "Let stand in microwave for 1 minute to set"
    ],

    "ingredients": [
        "2 tablespoons tahini",
        "2 tablespoons soybean oil (often labeled \"vegetable oil\")",
        "3 tablespoons lemon juice",
        "2 tablespoons water",
        "1 tablespoon minced garlic",
        "1 tablespoon soy sauce",
        "2 teaspoons sugar",
        "8 cups thinly sliced kale, packed",
        "1  cucumber, peeled and sliced",
        "\u00bd cup chopped smoked almonds",
        "\u00bd cup golden raisins"
    ],
    "directions": [
        "Combine tahini, soybean oil, lemon juice, water, garlic, soy sauce and sugar in small bowl",
        "Stir until smooth",
        "Place kale and cucumbers in large bowl",
        "Pour dressing over kale; toss until combined",
        "Marinate for a minimum of 20 minutes, tossing occasionally",
        "Sprinkle with almonds and golden raisins"
    ],

        "\u00bc cup butter",
        "2 pounds ground beef",
        "2 teaspoons salt",
        "\u00bd teaspoon ground allspice",
        "\u00bd teaspoon ground cinnamon",
        "\u00bd teaspoon ground black pepper",
        "4\u2009\u00bd cups chicken broth",
        "2 cups long grain white rice",
        "2 tablespoons butter",
        "\u00bd cup pine nuts"

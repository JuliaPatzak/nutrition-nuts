    "ingredients": [
        "6 slices bacon",
        "1 (10 ounce) package frozen broccoli",
        "2 tablespoons water",
        "2 cups shredded Cheddar cheese",
        "\u00bd cup mayonnaise",
        "\u00bc teaspoon ground white pepper",
        "\u00bd cup sour cream",
        "1 tablespoon yellow mustard"
    ],
    "directions": [
        "Place bacon in a large, deep skillet",
        "Cook over medium high heat until evenly brown",
        "Drain, crumble and set aside",
        "Place broccoli and water in a medium saucepan",
        "Cover and bring to a boil",
        "Cook 5 minutes",
        "Uncover, continue cooking 2 to 3 minutes, until tender",
        "Remove from heat and drain",
        "In a medium saucepan, mix the Cheddar cheese, mayonnaise, white pepper, sour cream and mustard",
        "Cook and stir over medium heat until well blended and warm",
        "Stir in the crumbled bacon and cooked broccoli",
        "Transfer to a medium dish to serve"
    ],

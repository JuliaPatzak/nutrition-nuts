    "ingredients": [
        "1 pound dried split peas",
        "5 cups water, or more if needed",
        "3 pounds smoked turkey legs",
        "3 cups chicken broth",
        "1\u2009\u00bd cups chopped carrot",
        "1 cup chopped celery",
        "2  potatoes, peeled and chopped",
        "1  onion, chopped",
        "\u00bd teaspoon garlic powder",
        "\u00bd teaspoon dried oregano",
        "2  bay leaves"
    ],
    "directions": [
        "Combine the split peas, water, smoked turkey legs, chicken broth, carrot, celery, potatoes, onion, garlic powder, oregano, and bay leaves in a slow cooker",

    "ingredients": [
        "1 (9 inch) pie shell",
        "\u00bc cup butter, softened",
        "1 cup white sugar",
        "2  eggs",
        "2 cups applesauce",
        "\u00bc teaspoon vanilla extract",
        "\u00bc teaspoon ground cinnamon"
    ],
    "directions": [
        "Preheat oven to 425 degrees F (220 degrees C)",
        "In a medium mixing bowl combine butter or margarine, sugar, and eggs",
        "Beat until smooth",
        "Add applesauce and vanilla",
        "Mix thoroughly",
        "Pour custard mixture into pie shell",
        "Sprinkle top lightly with cinnamon",
        "Bake in preheated oven for 30 to 40 minutes or until set in center"
    ],

    "ingredients": [
        "2 tablespoons olive oil, or to taste",
        "\u00bd  French baguette, halved lengthwise",
        "1 clove garlic, minced",
        "salt and ground black pepper to taste",
        "1  tomato, thinly sliced",
        "1 teaspoon dried parsley",
        "1 teaspoon dried basil",
        "\u00bd (8 ounce) package mozzarella cheese, thinly sliced",
        "6  fresh basil leaves",
        "1 sprig fresh parsley  (Optional)"
    ],
    "directions": [
        "Preheat an outdoor grill for low heat and lightly oil the grate",
        "Drizzle olive oil over baguette halves",
        "Add minced garlic and season with salt and pepper",
        "Layer tomato on top; season tomato with salt, pepper, dried parsley, and dried basil",
        "Top tomato with mozzarella cheese",
        "Place baguette halves on foil and grill until cheese starts to melt, 5 to 8 minutes",
        "Garnish with fresh basil and parsley",
        "Cut into slices and serve"
    ],

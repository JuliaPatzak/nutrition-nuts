    "ingredients": [
        "2 cups plain yogurt",
        "1  egg, beaten",
        "\u2153 cup molasses",
        "1 cup raisins",
        "\u00bd cup wheat germ",
        "1 cup quick cooking oats",
        "3 tablespoons butter, melted",
        "1\u2009\u00bd cups whole wheat flour",
        "1\u2009\u00bc teaspoons baking soda",
        "1 teaspoon ground cinnamon",
        "\u00bd teaspoon ground ginger",
        "\u00bc teaspoon ground cloves",
        "\u00bd teaspoon salt"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease 12 muffin cups",
        "Beat together the yogurt, egg, molasses, raisins, wheat germ, oats, and butter in a large bowl until evenly mixed",
        "Sift the flour, baking soda, cinnamon, ginger, cloves, and salt on top of the molasses mixture",
        "Fold the flour mixture into the molasses until a batter has formed",
        "Pour batter into muffin cups, filling to the top",
        "Bake in preheated oven until a toothpick inserted into the middle of a muffin comes out clean, about 25 minutes"
    ],

    "ingredients": [
        "1 cup pumpkin puree",
        "1  egg, lightly beaten",
        "1 cup all-purpose flour",
        "1 teaspoon baking powder",
        "1 teaspoon curry powder",
        "1 teaspoon salt",
        "4 cups vegetable oil for frying"
    ],
    "directions": [
        "In a medium bowl, combine pumpkin, egg, flour, baking powder, curry powder, and salt",
        "Mix until smooth",
        "Heat oil in a deep saucepan to 325 degrees F (165 degrees C)",
        "Drop batter by spoonfuls into hot oil",
        "Fry until golden brown, about 2 minutes",
        "Remove with a slotted spoon, and serve immediately"
    ],

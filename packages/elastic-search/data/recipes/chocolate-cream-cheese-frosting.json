    "ingredients": [
        "4 (1 ounce) squares unsweetened chocolate",
        "1 (8 ounce) package cream cheese",
        "4 cups confectioners' sugar",
        "1 tablespoon vanilla extract",
        "\u215b teaspoon salt",
        "2 tablespoons evaporated milk"
    ],
    "directions": [
        "Have all ingredients at room temperature",
        "Melt the chocolate in a double boiler or microwave oven",
        "In a large bowl, beat cream cheese until fluffy",
        "Pour melted chocolate into cream cheese and mix on medium speed until well blended",
        "Add vanilla and pinch of salt",
        "Slowly mix in the confectioners sugar",
        "Scrape the sides of the bowl and increase mixer speed to medium-high",
        "Slowly add evaporated milk until the frosting is spreading consistency"
    ],

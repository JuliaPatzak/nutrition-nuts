    "ingredients": [
        "2 (14.5 ounce) cans chicken broth",
        "1 (14.5 ounce) can beef broth",
        "1 (18 ounce) package frozen meatballs",
        "1 (16 ounce) package frozen mixed vegetables",
        "1 (14.5 ounce) can stewed tomatoes",
        "\u00bd cup elbow macaroni",
        "1 (15 ounce) can light red kidney beans, drained and rinsed",
        "1\u2009\u00bd teaspoons Italian seasoning",
        "6 tablespoons grated Parmesan cheese"
    ],
    "directions": [
        "Heat chicken broth and beef broth in a large Dutch oven or stock pot; add meatballs, vegetables, tomatoes, and macaroni",
        "Cover Dutch oven and bring to a boil, about 10 minutes",
        "Remove cover and stir",
        "Stir kidney beans and Italian seasoning into soup; reduce heat to medium",
        "Simmer, stirring frequently, until macaroni is tender, 6 to 7 minutes",
        "Ladle soup into 6 bowls; garnish each with 1 tablespoon Parmesan cheese"
    ],

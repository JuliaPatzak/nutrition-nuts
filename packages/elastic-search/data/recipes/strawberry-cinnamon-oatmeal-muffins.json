    "ingredients": [
        "1 cup all-purpose flour",
        "1 cup rolled oats",
        "2 teaspoons baking powder",
        "\u00bd teaspoon baking soda",
        "\u00bd teaspoon ground cinnamon",
        "\u00bd teaspoon salt",
        "1  egg, beaten",
        "\u00bd cup milk",
        "\u00bc cup light olive oil",
        "\u00bd cup sugar",
        "\u00bd pint chopped fresh strawberries"
    ],
    "directions": [
        "Preheat oven to 425 degrees F (220 degrees C)",
        "Grease 12 muffin cups",
        "In a large bowl, mix flour, oats, baking powder, baking soda, cinnamon and salt",
        "In a separate bowl, whisk together egg, milk, olive oil and sugar",
        "Mix in strawberries",
        "Stir strawberry mixture into oat mixture just until evenly moist",
        "Spoon into prepared muffin cups",
        "Bake 18 to 20 minutes in the preheated oven, until a knife inserted in the center of a muffin comes out clean"
    ],

    "ingredients": [
        "1 (16 ounce) can crabmeat",
        "2 (8 ounce) packages cream cheese, softened",
        "1 (8 ounce) package sharp Cheddar cheese spread",
        "3 tablespoons lemon juice",
        "3 tablespoons Worcestershire sauce",
        "3 tablespoons beer",
        "2 tablespoons chopped fresh chives",
        "2 tablespoons grated onion",
        "1 tablespoon chile-garlic sauce (such as Sriracha\u00ae)",
        "1 teaspoon cayenne pepper",
        "1 clove garlic, crushed",
        "1 pinch paprika, or to taste"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Mix crabmeat, cream cheese, Cheddar cheese spread, lemon juice, Worcestershire sauce, beer, chives, onion, chile-garlic sauce, cayenne pepper, and garlic together in a bowl; spread evenly into a 9x9-inch baking dish",
        "Sprinkle crab-cheese mixture with paprika",
        "Cover baking dish with aluminum foil",
        "Bake in the preheated oven for 20 minutes; remove aluminum foil and bake until bubbling, about 5 more minutes"
    ],

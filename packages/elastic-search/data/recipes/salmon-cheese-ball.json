    "ingredients": [
        "1 (7 ounce) can salmon, drained and flaked",
        "1 (8 ounce) package cream cheese, softened",
        "1 tablespoon lemon juice",
        "3 tablespoons finely chopped green onions",
        "2 teaspoons prepared horseradish",
        "\u00bc teaspoon salt",
        "\u00bc teaspoon hot pepper sauce",
        "\u00bc teaspoon Worcestershire sauce",
        "\u00bd cup chopped walnuts",
        "\u00bc cup chopped fresh parsley"
    ],
    "directions": [
        "In a medium bowl, mix salmon, cream cheese, lemon juice, green onions, horseradish, salt, hot pepper sauce and Worcestershire sauce",
        "Form the mixture into a ball and wrap in plastic",
        "Chill in the refrigerator 3 hours, or until firm",
        "On a large piece of wax paper, mix walnuts and fresh parsley",
        "Roll the ball in the walnuts and parsley until thoroughly coated",
        "Chill in the refrigerator until serving"
    ],

    "ingredients": [
        "8 cups cubed bread",
        "1 cup raisins  (Optional)",
        "2 cups milk",
        "4  eggs",
        "\u00bc cup butter, melted",
        "\u00bc cup white sugar",
        "\u00bd teaspoon vanilla extract",
        "\u00bc teaspoon ground nutmeg"
    ],
    "directions": [
        "Place bread and raisins in a slow cooker",
        "Whisk milk, eggs, butter, sugar, vanilla extract, and nutmeg together in a bowl; pour over bread and raisins",

    "ingredients": [
        "\u00bd cup almond milk",
        "2 tablespoons margarine",
        "4 cups confectioners' sugar",
        "1 cup dark chocolate chips",
        "\u00bd cup cocoa powder",
        "\u00bd teaspoon vanilla extract",
        "\u00bd teaspoon cayenne pepper",
        "\u00bd teaspoon ground cinnamon"
    ],
    "directions": [
        "Stir milk and margarine together in a saucepan; bring to a simmer over medium-low heat, about 5 minutes",
        "Combine confectioners' sugar, chocolate chips, cocoa powder, vanilla extract, cayenne pepper, and ground cinnamon together in a bowl",
        "Pour hot milk mixture over sugar mixture and stir until smooth; spread into a glass baking dish",
        "Refrigerate fudge until firm, at least 30 minutes"
    ],

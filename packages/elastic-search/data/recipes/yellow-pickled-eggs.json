    "ingredients": [
        "12  eggs",
        "1 medium onion, thinly sliced",
        "\u00be cup water",
        "1 cup white sugar",
        "1\u2009\u00bc cups white vinegar",
        "2 teaspoons salt",
        "1 teaspoon dried dill weed",
        "\u00bc teaspoon garlic powder",
        "\u00bd teaspoon mustard seed"
    ],
    "directions": [
        "Place eggs in a large pot and fill with enough water to cover",
        "Bring to a boil, then cover and remove from heat",
        "Let stand 10 minutes",
        "Cool under running water, and remove shells",
        "Place hard-cooked eggs in a large jar",
        "In a saucepan, combine the onion, water, sugar, and vinegar",
        "Stir in salt, dill, garlic powder, and mustard seed",
        "Bring to a boil, then simmer over low heat for 5 to 7 minutes",
        "Pour over the eggs in the jar",
        "Cover and refrigerate for at least 4 days"
    ],

    "ingredients": [
        "\u00bd medium red onion, minced",
        "3 tablespoons lime juice",
        "2 large  fresh peaches - peeled, pitted, and diced",
        "\u00bd cup cucamelons, thinly sliced",
        "4 sprigs fresh cilantro, chopped",
        "1  green chile pepper, seeded and minced  (Optional)",
        "\u00bc teaspoon chili powder",
        "1 pinch salt"
    ],
    "directions": [
        "Combine onion and lime juice in a bowl and marinate for 15 minutes",
        "Add peaches, cucamelons, cilantro, chile pepper, chili powder and salt to the bowl with the onion",
        "Stir to combine",
        "Chill until serving"

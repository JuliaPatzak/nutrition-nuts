    "ingredients": [
        "1 tablespoon olive oil",
        "1 large onion, chopped",
        "3 cloves garlic, peeled and chopped",
        "2 (10 ounce) packages chopped frozen broccoli, thawed",
        "1  potato, peeled and chopped",
        "4 cups chicken broth",
        "\u00bc teaspoon ground nutmeg",
        "salt and pepper to taste"
    ],
    "directions": [
        "Heat olive oil in a large saucepan, and saute onion and garlic until tender",
        "Mix in broccoli, potato, and chicken broth",
        "Bring to a boil, reduce heat, and simmer 15 minutes, until vegetables are tender",
        "With a hand mixer or in a blender, puree the mixture until smooth",
        "Return to the saucepan, and reheat",
        "Season with nutmeg, salt, and pepper"

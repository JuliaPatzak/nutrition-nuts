    "ingredients": [
        "\u00bd pound bacon",
        "1 teaspoon minced garlic",
        "3 (8 ounce) packages cream cheese, softened",
        "4 ounces blue cheese",
        "\u00bc cup chopped walnuts or pecans to taste"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "In a large skillet, fry chopped bacon until it is almost done",
        "Stir garlic into the skillet",
        "Remove from skillet and drain excess fat",
        "In a medium mixing bowl, mix bacon with cream cheese and blue cheese",
        "Transfer the mixture to a casserole dish",
        "Sprinkle nuts over the dip",
        "Bake for 30 to 40 minutes"
    ],

    "ingredients": [
        "1 (16 ounce) package firm tofu",
        "1 pinch sea salt to taste",
        "1  egg, lightly beaten",
        "1 cup vegetable oil for frying",
        "\u2153 cup soy sauce",
        "2  green onions, chopped",
        "2  serrano chile peppers, chopped",
        "2 teaspoons white vinegar",
        "2 teaspoons white sugar",
        "1 teaspoon red pepper flakes"
    ],
    "directions": [
        "Slice tofu into 12 equal rectangular pieces",
        "Sprinkle salt on both sides of tofu slices",
        "Heat vegetable oil in a large, deep saucepan over medium heat",
        "Dip tofu slices into bowl of beaten egg and fry in hot oil until lightly browned, about 3 minutes per side",
        "Mix soy sauce, green onions, chile peppers, vinegar, sugar, and red pepper flakes together in a small bowl",
        "Dip fried tofu into dipping sauce"
    ],

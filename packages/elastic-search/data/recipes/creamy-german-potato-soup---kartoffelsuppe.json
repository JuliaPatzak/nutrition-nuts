    "ingredients": [
        "2 tablespoons vegetable oil",
        "2  onions, halved",
        "2  carrots, peeled and cubed",
        "\u00bd  celeriac (celery root), peeled and cubed",
        "2 pounds potatoes, peeled and cubed",
        "8 cups vegetable stock",
        "\u00bc bunch fresh parsley, chopped",
        "1 cup half-and-half",
        "2 teaspoons salt",
        "1 teaspoon ground black pepper",
        "1 teaspoon ground cumin",
        "4  tomatoes, seeded and chopped",
        "4  hot dogs, sliced  (Optional)"
    ],
    "directions": [
        "Heat vegetable oil in large pot over medium heat; cook and stir onions, carrots, and celeriac until beginning to soften, about 5 minutes",
        "Stir in potatoes and vegetable stock",
        "Simmer until potatoes are tender, 15 to 20 minutes",
        "Add parsley and blend potato mixture with a stick blender until soup is smooth",
        "Stir half-and-half, salt, black pepper, and cumin into soup",
        "Ladle soup in bowls and garnish with tomatoes and hot dogs"
    ],

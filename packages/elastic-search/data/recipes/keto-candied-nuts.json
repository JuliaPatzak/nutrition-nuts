    "ingredients": [
        "1  egg white",
        "1 tablespoon water",
        "1 cup pecans",
        "\u00bc cup monk fruit sugar substitute, or more to taste",
        "1 teaspoon ground cinnamon",
        "1 teaspoon Himalayan sea salt"
    ],
    "directions": [
        "Preheat the oven to 250 degrees F (120 degrees C)",
        "Beat egg white and water in a glass, metal, or ceramic bowl with an electric mixer until peaks form",
        "Stir in pecans to coat",
        "Combine monk fruit sugar substitute, cinnamon, and sea salt in a small bowl",
        "Sprinkle over pecans and mix to combine",
        "Spread out on a baking sheet",
        "Bake in the preheated oven for about 1 hour, stirring every 15 minutes"
    ],

    "ingredients": [
        "2 teaspoons celery salt",
        "2 teaspoons ground black pepper",
        "1\u2009\u00bd teaspoons salt",
        "1 teaspoon garlic salt",
        "1 teaspoon onion salt",
        "2 teaspoons Worcestershire sauce",
        "3 pounds beef brisket",
        "1 cup Russian dressing",
        "1 cup hickory-flavored barbeque sauce"
    ],
    "directions": [
        "Mix celery salt, black pepper, salt, garlic salt, and onion salt in a small bowl; add Worcestershire sauce",
        "Spread mixture over beef brisket; transfer to a slow cooker",
        "Cook on Low until beef is tender, about 8 hours",
        "Transfer beef to a cutting board; shred into small pieces using 2 forks",
        "Measure 1/2 cup cooking liquid from the slow cooker into a saucepan",
        "Mix in Russian dressing and barbeque sauce; bring to a boil",
        "Combine shredded beef and sauce mixture in the slow cooker",
        "Cook on Low until flavors combine, about 1 hour"
    ],

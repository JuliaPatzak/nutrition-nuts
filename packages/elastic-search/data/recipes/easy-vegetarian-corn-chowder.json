    "ingredients": [
        "6 tablespoons butter",
        "\u00bc cup diced onion",
        "\u00bd cup diced celery",
        "6 tablespoons all-purpose flour",
        "2 (14.5 ounce) cans vegetable broth",
        "2 (15 ounce) cans creamed corn",
        "1 (15 ounce) can whole kernel corn, drained",
        "2 tablespoons shredded carrot",
        "1 cup half-and-half cream",
        "\u00be cup skim milk",
        "\u00bd teaspoon ground nutmeg",
        "\u00bc teaspoon ground black pepper",
        "1 pinch salt"
    ],
    "directions": [
        "In a large saucepan over medium heat, melt butter",
        "Cook onions and celery in butter 3 minutes",
        "Whisk in flour and cook 6 minutes more, until light brown",
        "Whisk in broth and simmer 10 minutes",
        "Stir in creamed corn, corn, carrot, half-and-half, milk, nutmeg, pepper and salt",
        "Simmer over low heat 10 minutes more"
    ],

    "ingredients": [
        "2 cups raw pumpkin seeds",
        "2 tablespoons olive oil, or as needed",
        "1 tablespoon minced garlic",
        "1 tablespoon salt",
        "\u00bd teaspoon cayenne pepper",
        "1 pinch ground black pepper to taste"
    ],
    "directions": [
        "Preheat the oven to 250 degrees F (120 degrees C)",

    "ingredients": [
        "2  eggs, beaten",
        "1 cup white sugar",
        "1 cup chopped dates",
        "1 teaspoon vanilla extract",
        "1 tablespoon butter",
        "3 cups crispy rice cereal",
        "1 cup flaked coconut"
    ],
    "directions": [
        "In a medium saucepan over medium heat, combine the eggs sugar and dates",
        "Bring to a boil, stirring occasionally, and boil for 5 minutes",
        "Remove from heat and stir in the butter and vanilla",
        "Mix in crispy rice cereal",
        "Butter hands and roll the mixture into walnut sized balls",
        "Roll the balls in coconut",
        "Serve"
    ],

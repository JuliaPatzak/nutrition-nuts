    "ingredients": [
        "1 (10 ounce) package shredded purple cabbage",
        "1 (10 ounce) package broccoli coleslaw mix",
        "1 (8 ounce) container grape tomatoes, halved",
        "2 tablespoons rice vinegar",
        "1 tablespoon olive oil",
        "\u00bd teaspoon white sugar",
        "\u00bc teaspoon salt",
        "\u215b teaspoon garlic powder",
        "\u215b teaspoon ground black pepper",
        "3  scallions, sliced",
        "\u00bc cup dried cranberries",
        "\u00bc cup sliced almonds"
    ],
    "directions": [
        "Place cabbage, broccoli coleslaw mix, and tomatoes in a large bowl",
        "Whisk vinegar, oil, sugar, salt, garlic powder, and black pepper together in a small bowl until combined",
        "Pour over the coleslaw to coat; stir in scallions, cranberries, and almonds",
        "Cover with plastic wrap and refrigerate until flavors meld, 8 hours to overnight"
    ],

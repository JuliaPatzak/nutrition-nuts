    "ingredients": [
        "1\u2009\u00bd cups instant rice",
        "1\u2009\u00bd cups water",
        "1 clove garlic, minced",
        "2 tablespoons finely chopped onion",
        "\u00bd teaspoon salt",
        "\u00bd teaspoon freshly ground black pepper",
        "1 (5 ounce) can tuna, drained",
        "\u00bd cup mozzarella cheese, shredded",
        "\u00bc cup tomatoes, chopped"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "In a medium saucepan, mix water, garlic, onion, salt and black pepper",
        "Cover and bring to a boil",
        "Let boil for 1 minute, then stir in the rice",
        "Cover and remove from heat",
        "Let stand for 5 minutes",
        "Line the bottom of a 9x5 inch loaf pan with half of the rice",
        "Top with drained tuna, cheese, tomatoes and the remaining rice",
        "Bake in a preheated oven for 20 minutes"
    ],

    "ingredients": [
        "5 peppers green chile peppers, chopped",
        "3 tablespoons chopped shallot",
        "2 tablespoons chopped garlic",
        "1 tablespoon chopped galangal",
        "2 teaspoons salt, or to taste",
        "2 teaspoons chopped lemon grass",
        "2 teaspoons chopped fresh cilantro, or more to taste",
        "1 teaspoon finely chopped makrut lime leaves",
        "1 teaspoon whole black peppercorns",
        "1 teaspoon cumin seeds",
        "1 teaspoon coriander seeds",
        "1 tablespoon shrimp paste"
    ],
    "directions": [
        "Put chile peppers, shallot, garlic, galangal, salt, lemon grass, cilantro, lime leaves, peppercorns, cumin seeds, and coriander seeds in a mortar",
        "Grind with a pestle until combined",
        "Add shrimp paste and grind until paste is smooth"

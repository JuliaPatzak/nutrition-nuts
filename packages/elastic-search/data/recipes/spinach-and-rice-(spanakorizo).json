    "ingredients": [
        "\u2153 cup olive oil",
        "2  onions, chopped",
        "2 pounds fresh spinach, rinsed and stemmed",
        "1 (8 ounce) can tomato sauce",
        "2 cups water",
        "1 teaspoon dried dill weed",
        "1 teaspoon dried parsley",
        "salt and pepper to taste",
        "\u00bd cup uncooked white rice"
    ],
    "directions": [
        "Heat olive oil in a large skillet over medium-high heat",
        "Saute onions in the oil until soft and translucent",
        "Add spinach, and cook stirring for a few minutes, then pour in the tomato sauce and water",
        "Bring to a boil, and season with parsley, dill, salt and pepper",
        "Stir in rice, reduce heat to low, and simmer uncovered for 20 to 25 minutes, or until rice is tender",
        "Add more water if necessary"
    ],

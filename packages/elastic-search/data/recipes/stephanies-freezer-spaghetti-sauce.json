    "ingredients": [
        "4  onions, chopped",
        "4 cloves garlic, minced",
        "1  green bell pepper, chopped",
        "\u00bd cup vegetable oil",
        "16 cups chopped tomatoes",
        "2 tablespoons dried oregano",
        "2 tablespoons dried basil",
        "\u00bc cup chopped parsley",
        "\u00bc cup white sugar",
        "2 tablespoons salt",
        "\u00be teaspoon ground black pepper",
        "1 (6 ounce) can tomato paste"
    ],
    "directions": [
        "In a slow cooker saute together the onion, garlic, green pepper, and vegetable oil",
        "Cook until onion is transparent",
        "Add the chopped tomatoes, oregano, basil, parsley, sugar, salt, and ground black pepper",
        "Cook for 2 to 3 hours on low heat",
        "Stir frequently",
        "Let sauce cool",
        "Pour sauce into quart size freezer containers",
        "Store in freezer",
        "When ready to use sauce, stir in can of tomato paste"

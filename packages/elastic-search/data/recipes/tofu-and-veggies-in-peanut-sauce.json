    "ingredients": [
        "1 tablespoon peanut oil",
        "1 small head broccoli, chopped",
        "1 small red bell pepper, chopped",
        "5  fresh mushrooms, sliced",
        "1 pound firm tofu, cubed",
        "\u00bd cup peanut butter",
        "\u00bd cup hot water",
        "2 tablespoons vinegar",
        "2 tablespoons soy sauce",
        "1\u2009\u00bd tablespoons molasses",
        "ground cayenne pepper to taste"
    ],
    "directions": [
        "Heat oil in a large skillet or wok over medium-high heat",
        "Saute broccoli, red bell pepper, mushrooms and tofu for 5 minutes",
        "In a small bowl combine peanut butter, hot water, vinegar, soy sauce, molasses and cayenne pepper",
        "Pour over vegetables and tofu",
        "Simmer for 3 to 5 minutes, or until vegetables are tender crisp"

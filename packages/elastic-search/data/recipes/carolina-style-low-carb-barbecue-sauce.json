    "ingredients": [
        "1 cup prepared yellow mustard",
        "\u00bd cup granular no-calorie sucralose sweetener (e.g., Splenda \u00ae)",
        "\u00be cup cider vinegar",
        "2 tablespoons chili powder",
        "1 teaspoon ground black pepper",
        "1 teaspoon ground white pepper",
        "\u00bc teaspoon cayenne pepper",
        "\u00bd teaspoon soy sauce",
        "2 tablespoons butter",
        "1 tablespoon liquid smoke flavoring"
    ],
    "directions": [
        "In a saucepan over medium heat, stir together the mustard, sweetener, cider vinegar, chili powder, black pepper, white pepper, and cayenne pepper",
        "Simmer for 30 minutes",
        "Stir in the soy sauce, butter, and liquid smoke; simmer for 10 more minutes",
        "Cool completely, and refrigerate overnight to blend flavors before using"

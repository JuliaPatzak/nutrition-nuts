    "ingredients": [
        "3 cups shredded lettuce",
        "1 (15 ounce) can black beans, rinsed and drained",
        "2  chopped fresh red chile peppers",
        "\u00bc cup sliced green onions",
        "1 (8 ounce) container sour cream",
        "2  fresh jalapeno peppers, finely diced",
        "1 teaspoon grated lime zest",
        "1 cup chunky salsa",
        "\u00bd  avocado - peeled, pitted and diced",
        ".66 cup shredded Monterey Jack cheese",
        "\u2153 cup chopped black olives, drained",
        "1 tablespoon chopped fresh cilantro"
    ],
    "directions": [
        "Layer shredded lettuce on a 12 inch serving platter",
        "In a medium bowl, stir together black beans, red chile peppers and green onions",
        "Spoon the mixture on top of the lettuce",
        "Gently spread sour cream over beans",
        "Sprinkle with jalapeno peppers and lime zest",
        "Drain excess liquid from salsa",
        "Place salsa in a medium bowl",
        "Stir in avocado",
        "Spoon the mixture over sour cream layer",
        "Sprinkle with Monterey Jack cheese",
        "Top with black olives and cilantro",
        "Serve immediately, or chill in the refrigerator"
    ],

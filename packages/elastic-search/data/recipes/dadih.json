    "ingredients": [
        "2 cups whole milk",
        "2 cups water",
        "\u00bc cup white sugar",
        "\u215c teaspoon agar-agar powder",
        "\u00be teaspoon strawberry extract",
        "2 drops red food coloring"
    ],
    "directions": [
        "Combine milk, water, sugar, and agar-agar in a saucepan",
        "Bring to a boil, stirring occasionally",
        "Simmer for 2 minutes",
        "Remove from heat; remove any skin that formed on the surface",
        "Stir strawberry extract and red food coloring into the milk mixture",
        "Pour mixture into small plastic cups",
        "Let cool, about 10 minutes",
        "Chill until set, about 1 hour"
    ],

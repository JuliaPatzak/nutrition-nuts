    "ingredients": [
        "1\u2009\u00bd cups water",
        "2  whole cardamom pods, broken",
        "1 pinch saffron powder",
        "2  tea bags",
        "1 teaspoon white sugar  (Optional)"
    ],
    "directions": [
        "Combine the water, saffron, and cardamom in a saucepan over medium heat",
        "Cover, and bring to a boil",
        "Add tea bags, and let the tea steep for a minute, or longer if you like stronger tea",
        "Strain into a cup, and sweeten with sugar if desired"

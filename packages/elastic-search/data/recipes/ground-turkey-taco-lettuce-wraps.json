    "ingredients": [
        "1 tablespoon grapeseed oil",
        "1 pound ground turkey breast",
        "1 small yellow onion, chopped",
        "5  mushrooms, halved and sliced",
        "3 cloves garlic, or more to taste, diced",
        "\u00bd teaspoon chili powder",
        "\u00bd teaspoon ground cumin",
        "\u00bd teaspoon paprika",
        "\u00bd teaspoon sea salt",
        "\u00bc teaspoon dried thyme",
        "\u00bc teaspoon ground black pepper",
        "1 (15 ounce) can baked beans in tomato sauce",
        "1 tablespoon ketchup",
        "\u00bd teaspoon gluten-free Worcestershire sauce",
        "\u00bd teaspoon gluten-free soy sauce (tamari)",
        "6  romaine lettuce leaves, or to taste",
        "\u00bd cup chopped mushrooms",
        "\u00bd cup diced red bell pepper",
        "1  tomato, diced",
        "2  green onions, or more to taste, chopped"
    ],
    "directions": [
        "Heat a large frying pan over medium-high heat",
        "Add grapeseed oil, turkey, and onion; fry for 5 minutes",
        "Add sliced mushrooms and garlic",
        "Cook until turkey is browned, 5 to 10 minutes more",
        "Reduce heat to medium-low",
        "Add chili, cumin, paprika, salt, thyme, and pepper; stir for 1 minute",
        "Add beans, ketchup, Worcestershire sauce, and soy sauce",
        "Cook and stir for 5 minutes",
        "Spoon turkey mixture into lettuce leaves",
        "Top with chopped mushrooms, bell pepper, tomato, and green onions"
    ],

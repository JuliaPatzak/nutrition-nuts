    "ingredients": [
        "cooking spray (such as Pam\u00ae)",
        "1 head cauliflower, run through a ricer",
        "1 cup shredded mozzarella cheese",
        "\u00bd  red bell pepper, diced",
        "1  egg, or more if needed",
        "1 teaspoon minced garlic  (Optional)",
        "1 teaspoon garlic salt",
        "\u00bd teaspoon ground black pepper",
        "\u00bd teaspoon ground nutmeg",
        "1 cup shredded pepperjack cheese"
    ],
    "directions": [
        "Preheat oven to 425 degrees F (220 degrees C)",
        "Line a baking sheet with parchment paper and spray with cooking spray",
        "Mix cauliflower, mozzarella cheese, red bell pepper, egg, garlic, garlic salt, black pepper, and nutmeg together in a large bowl until mixture holds together",
        "Spread mixture in a thin layer onto the prepared baking sheet",
        "Bake in the preheated oven for 12 minutes",
        "Sprinkle pepperjack cheese over top and continue baking until cheese is bubbling, 2 to 3 minutes more",
        "Cool and cut into 3- to 4-inch squares"
    ],

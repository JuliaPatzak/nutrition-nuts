    "ingredients": [
        "1 cup canned shrimp",
        "2  hard-cooked eggs, chopped",
        "1\u2009\u00bd cups finely grated carrots",
        "1 tablespoon minced onion",
        "\u00bd teaspoon salt",
        "\u215b teaspoon ground black pepper",
        "\u00bd cup mayonnaise",
        "2 tablespoons lemon juice",
        "\u00bd teaspoon prepared mustard",
        "4  medium tomatoes",
        "4  leaves of lettuce"
    ],
    "directions": [
        "Add shrimp, eggs, carrots, onion, salt, and pepper to the bowl of a food processor",
        "Pulse until just combined",
        "Blend mayonnaise with lemon juice and mustard; mix into shrimp mixture with a fork",
        "Core tomatoes",
        "Cut into quarters without cutting all the way to the bottom",
        "Place each tomato on a lettuce leaf, and open",
        "Spoon shrimp mixture into the center"

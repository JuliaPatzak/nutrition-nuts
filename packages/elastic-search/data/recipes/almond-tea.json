    "ingredients": [
        "3 tablespoons instant iced tea powder",
        "1 cup white sugar",
        "2 cups boiling water",
        "1 (12 ounce) can frozen lemonade concentrate",
        "2 teaspoons vanilla extract",
        "1 tablespoon almond extract"
    ],
    "directions": [
        "In a 1 gallon container, mix together the instant tea powder and sugar",
        "Pour in the boiling water and lemonade concentrate, and mix well",
        "Stir in the vanilla and almond extracts",
        "Fill container the rest of the way with cold water",
        "Stir and serve over ice, or refrigerate until ready to serve"
    ],

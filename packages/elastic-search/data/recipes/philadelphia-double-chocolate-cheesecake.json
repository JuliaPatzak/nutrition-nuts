    "ingredients": [
        "24 each OREO Cookies, crushed",
        "\u00bc cup butter or margarine, melted",
        "4 (8 ounce) packages PHILADELPHIA Cream Cheese, softened",
        "1 cup sugar",
        "2 tablespoons flour",
        "1 teaspoon vanilla",
        "1 (8 ounce) package BAKER'S Semi-Sweet Chocolate, melted",
        "4  large eggs eggs",
        "\u00bd cup blueberries"
    ],
    "directions": [
        "Heat oven to 325 degrees F",
        "Mix crumbs  and  butter; press onto bottom of 13x9-pan lined with foil",
        "Bake 10 min",
        "Beat cream cheese, sugar, flour  and  vanilla with mixer until blended",
        "Add chocolate; mix well",
        "Add eggs, 1 at a time, mixing on low after each just until blended",
        "Pour over crust",
        "Bake 45 minutes or until center is almost set",
        "Cool",
        "Refrigerate 4 hours",
        "Use foil to lift cheesecake from pan",
        "Top with berries"
    ],

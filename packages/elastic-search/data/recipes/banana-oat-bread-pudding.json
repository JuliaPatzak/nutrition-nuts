    "ingredients": [
        "4 slices whole wheat bread",
        "1 cup rolled oats",
        "2\u2009\u00bd cups lowfat milk",
        "\u00bc cup butter, softened",
        "4  ripe bananas, sliced",
        "\u2153 cup brown sugar",
        "\u00bc cup raisins  (Optional)"

    "ingredients": [
        "8 cups peeled and sliced cucumbers",
        "1 small onion, sliced",
        "2 tablespoons salt",
        "1\u2009\u00bd cups white sugar",
        "\u00bd cup white vinegar"
    ],
    "directions": [
        "Combine cucumbers, onion, and salt in a bowl filled with water",
        "Refrigerate until vegetables are softened, about 2 hours; drain",
        "Combine sugar and vinegar in a saucepan; bring to a boil",
        "Remove from heat; cool to room temperature",
        "Pour liquid over cucumber mixture",
        "Freeze until frozen, 1 to 2 hours"
    ],

    "ingredients": [
        "1  sugar cube",
        "1 teaspoon water",
        "1 dash bitters",
        "2 fluid ounces whiskey (rye or bourbon)",
        "1  lemon twist",
        "ice cubes",
        "1  orange slice, for garnish",
        "1  maraschino cherry, for garnish"
    ],
    "directions": [
        "Muddle sugar cube, water, and bitters in an old fashioned glass for 1 minute",
        "Pour in whiskey and stir for an additional minute",
        "Squeeze the lemon twist over the glass and drop it in",
        "Add ice cubes",
        "Garnish with a slice of orange and a maraschino cherry; serve with a swizzle stick"
    ],

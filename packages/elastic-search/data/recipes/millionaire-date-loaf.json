    "ingredients": [
        "4\u2009\u00bd cups white sugar",
        "1 (12 fluid ounce) can evaporated milk",
        "1 (8 ounce) package chopped dates",
        "2 cups marshmallow creme",
        "2 cups chopped pecans",
        "1 teaspoon vanilla extract"
    ],
    "directions": [
        "Grease an 8x8 inch dish",
        "In a heavy saucepan over medium-high heat combine sugar and evaporated milk; boil 5 minutes",
        "Stir in dates and cook 4 minutes more, stirring constantly",
        "Remove from heat and stir in marshmallow creme, pecans and vanilla extract",
        "Beat until cool",
        "Pour into prepared pan",
        "Cool and cut into squares"
    ],

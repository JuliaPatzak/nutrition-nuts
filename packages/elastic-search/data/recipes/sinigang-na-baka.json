    "ingredients": [
        "2 tablespoons canola oil",
        "1 large onion, chopped",
        "2 cloves garlic, chopped",
        "1 pound beef stew meat, cut into 1 inch cubes",
        "1 quart water",
        "2 large tomatoes, diced",
        "\u00bd pound fresh green beans, rinsed and trimmed",
        "\u00bd medium head bok choy, cut into 1 1/2 inch strips",
        "1 head fresh broccoli, cut into bite size pieces",
        "1 (1.41 ounce) package tamarind soup base"
    ],
    "directions": [
        "Heat oil in medium stock pot",
        "Saute onion and garlic until tender",
        "Add beef to pot, and saute until browned",
        "Pour in water",
        "Bring to a boil, reduce heat, and simmer 20 to 30 minutes",
        "Place tomatoes and green beans in pot, and continue to simmer for 10 minutes",
        "Stir in bok choy, broccoli and tamarind soup mix",
        "Simmer for an additional 10 minutes"
    ],

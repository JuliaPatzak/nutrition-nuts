    "review_count": 1,
    "ingredients": [
        "2 tablespoons olive oil",
        "2 cloves garlic, coarsely chopped",
        "1 pound frozen cut asparagus",
        "2 tablespoons chopped pistachio nuts",
        "salt and pepper to taste",
        "\u00bd cup grated Parmesan cheese",
        "2 tablespoons capers"
    ],
    "directions": [
        "Heat the olive oil in a skillet over medium heat",
        "Stir in the garlic, and cook 1 minute until fragrant",
        "Add the cut asparagus, and continue cooking until tender, about 5 minutes",
        "Sprinkle in the pistachios, then season to taste with salt and pepper",
        "Cook 1 minute longer",
        "Place the asparagus mixture into a blender or food processor",
        "Add the Parmesan cheese and capers",
        "Process until pureed",
        "Cool before serving"
    ],

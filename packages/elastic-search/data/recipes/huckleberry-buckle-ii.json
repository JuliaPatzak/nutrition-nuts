    "ingredients": [
        "\u00bc cup butter",
        "\u00bd cup white sugar",
        "1 cup all-purpose flour",
        "1 teaspoon baking powder",
        "\u00bc teaspoon salt",
        "\u00bd cup milk",
        "2\u2009\u00bd cups huckleberries",
        "\u00be cup white sugar",
        "\u00bd cup boiling water",
        "1 tablespoon butter"

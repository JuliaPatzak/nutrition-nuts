    "ingredients": [
        "3 tablespoons self-rising flour",
        "2 tablespoons brown sugar",
        "1 tablespoon white sugar",
        "1 teaspoon vanilla sugar  (Optional)",
        "1  egg",
        "1 tablespoon olive oil",
        "1 tablespoon milk",
        "1  ripe banana, mashed",
        "3  crushed pecans, or more to taste  (Optional)"
    ],
    "directions": [
        "Combine flour, brown sugar, white sugar, and vanilla sugar in a large microwave-safe mug",
        "Add egg and mix well",
        "Stir in oil, milk, mashed banana, and pecans",
        "Mix well to incorporate all the flour",
        "Microwave at high power until cake has risen and is cooked through, 2 1/2 to 3 minutes",
        "Remove and allow to cool slightly before eating"
    ],

    "ingredients": [
        "3 cups all-purpose flour",
        "2 cups white sugar",
        "2 teaspoons baking soda",
        "2 teaspoons pumpkin pie spice",
        "1 teaspoon salt",
        "\u00bd teaspoon baking powder",
        "2 cups pumpkin puree",
        "\u2154 cup vegetable oil",
        "3  eggs, beaten"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease two 9x5 inch loaf pans",
        "Mix together flour, sugar, baking soda, pie spice, salt and baking powder",
        "Add pumpkin, oil and eggs; mix well",
        "Pour into loaf pans and bake for 1 hour or until inserted toothpick comes out clean",
        "Allow to cool for 15 minutes before removing from pans"

    "ingredients": [
        "2 tablespoons extra virgin olive oil",
        "1 large onion, halved and thinly sliced",
        "1 tablespoon curry powder",
        "sea salt to taste",
        "4 small zucchini, halved lengthwise and cut into 1 inch slices",
        "1 quart chicken stock"
    ],
    "directions": [
        "Heat the oil in a large pot",
        "Stir in the onion, and season with curry powder and salt",
        "Cook and stir until onion is tender",
        "Stir in zucchini, and cook until tender",
        "Pour in the chicken stock",
        "Bring to a boil",
        "Cover, reduce heat to low, and simmer 20 minutes",
        "Remove soup from heat",
        "Use a hand blender, or transfer in batches to a blender, and blend until almost smooth"
    ],

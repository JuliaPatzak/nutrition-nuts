    "ingredients": [
        "2 tablespoons olive oil",
        "1 cup uncooked brown rice",
        "\u00bd  red bell pepper, chopped",
        "1  onion, chopped",
        "2 teaspoons crushed garlic",
        "2 cups water",
        "1 (10 ounce) can diced tomatoes",
        "2 teaspoons chili powder",
        "1 teaspoon salt"
    ],
    "directions": [
        "Heat oil in a deep skillet over medium heat",
        "Add rice, red bell pepper, onion, and garlic",
        "Cook and stir until rice is browned and onion is tender, about 5 minutes",
        "Stir in water and diced tomatoes",
        "Season with chili powder and salt",
        "Cover and simmer until rice is cooked, about 30 minutes"
    ],

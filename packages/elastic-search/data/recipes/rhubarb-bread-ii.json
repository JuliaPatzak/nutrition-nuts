    "ingredients": [
        "1\u2009\u00bd cups brown sugar",
        "\u00bd cup vegetable oil",
        "1  egg",
        "1 cup buttermilk",
        "1 teaspoon salt",
        "1 teaspoon vanilla extract",
        "2\u2009\u00bd cups all-purpose flour",
        "1\u2009\u00bd cups rhubarb, cut into 1/2 inch pieces",
        "\u00bd cup chopped walnuts",
        "\u00bc cup white sugar",
        "1 teaspoon butter, melted",
        "1 teaspoon ground cinnamon"
    ],
    "directions": [
        "Preheat oven to 325 degrees F (165 degrees C)",
        "Lightly grease a 9x5 inch baking pan",
        "In a large mixing bowl, stir together brown sugar, oil, egg, buttermilk, salt and vanilla",
        "Gradually stir in flour just until moistened",
        "Fold in rhubarb and walnuts",
        "Pour batter into prepared pan",
        "Combine 1/4 cup sugar, melted butter and cinnamon; sprinkle on top of batter",
        "Bake in preheated oven for 60 to 75 minutes, or until a toothpick inserted into center of loaf comes out clean"
    ],

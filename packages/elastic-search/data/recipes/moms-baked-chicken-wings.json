    "ingredients": [
        "1 cup lemon juice",
        "\u00bc cup soy sauce",
        "\u00bc cup olive oil",
        "2 tablespoons crushed garlic",
        "1 tablespoon seasoned salt",
        "\u00bd tablespoon paprika",
        "\u00bd tablespoon ground turmeric",
        "3 pounds chicken wings",
        "1 pinch garlic salt, or to taste"
    ],
    "directions": [
        "Preheat the oven to 375 degrees F (190 degrees C)",
        "Mix lemon juice, soy sauce, olive oil, garlic, seasoned salt, paprika, and turmeric together in a bowl",
        "Add chicken wings; thoroughly massage marinade into the chicken",
        "Transfer chicken and marinade into a 14x8-inch aluminum baking container",
        "Season with garlic salt",
        "Cover the pan with aluminum foil",
        "Bake in the preheated oven for 50 minutes",
        "Remove the foil and continue baking until crisp and no longer pink in the centers, about 50 minutes more"
    ],

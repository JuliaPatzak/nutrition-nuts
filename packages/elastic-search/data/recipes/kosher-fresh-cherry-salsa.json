    "ingredients": [
        "2 tablespoons boiling water",
        "1 teaspoon red pepper flakes",
        "2 cups pitted and halved sweet cherries",
        "\u2153 cup finely chopped red bell pepper",
        "\u2153 cup finely chopped red onion",
        "2 tablespoons lime juice",
        "1 tablespoon white sugar",
        "1 teaspoon kosher salt",
        "1 tablespoon chopped fresh cilantro"
    ],
    "directions": [
        "Combine boiling water and red pepper flakes in a large bowl",
        "Let stand until pepper flakes soften, about 5 minutes",
        "Stir cherries, red bell pepper, red onion, lime juice, sugar, and salt into the bowl with red pepper flakes",
        "Let stand until flavors combine, about 30 minutes",
        "Stir in cilantro before serving"
    ],

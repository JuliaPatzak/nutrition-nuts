    "ingredients": [
        "2\u2009\u00bc cups all-purpose flour",
        "1\u2009\u00bd teaspoons CALUMET Baking Powder",
        "\u00bd teaspoon baking soda",
        "1 (8 ounce) package PHILADELPHIA Cream Cheese, softened",
        "\u00bc cup butter, softened",
        "1 cup sugar",
        "1 cup mashed fully ripe bananas",
        "2  eggs",
        "1 cup chopped PLANTERS Walnuts"
    ],
    "directions": [
        "Heat oven to 350 degrees F",
        "Mix flour, baking powder and baking soda",
        "Beat cream cheese, butter and sugar in large bowl with mixer until blended",
        "Add bananas and eggs; mix well",
        "Gradually add flour mixture, mixing well after each addition",
        "Stir in nuts",
        "Pour into greased and floured 9x5-inch loaf pan",
        "Bake 1 hour 10 min",
        "or until toothpick inserted in center comes out clean",
        "Cool 5 minutes; remove from pan to wire rack",
        "Cool completely"
    ],

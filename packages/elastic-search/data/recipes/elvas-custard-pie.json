    "ingredients": [
        "6  eggs",
        "1 cup white sugar",
        "1 teaspoon vanilla extract",
        "1 pinch salt",
        "\u00bd teaspoon nutmeg",
        "2 cups milk",
        "1 (9 inch) unbaked pie crust"
    ],
    "directions": [
        "Preheat oven to 475 degrees F (245 degrees C",
        ") In a large bowl, beat eggs and sugar",
        "Beat in vanilla, salt and nutmeg",
        "Stir in milk",
        "Pour into crust",
        "Bake in the preheated oven for 10 minutes",
        "Reduce temperature to 350 degrees F (175 degrees C) and bake for 25 to 35 minutes, or until crust is golden brown and filling is set"
    ],

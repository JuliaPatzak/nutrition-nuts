    "ingredients": [
        "2 pounds lean ground beef",
        "1 (10.5 ounce) can condensed French onion soup",
        "\u00bd cup fine dry bread crumbs",
        "\u00bd cup rolled oats",
        "1  egg, lightly beaten",
        "1 tablespoon dried parsley",
        "\u00bc teaspoon salt",
        "\u00bc teaspoon ground black pepper"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Combine ground beef, condensed French onion soup, bread crumbs, oats, beaten egg, dried parsley, salt, and pepper in a bowl",
        "Mix well",
        "Pack into a loaf pan",
        "Bake in the preheated oven until no longer pink in the center, about 1 hour and 15 minutes",
        "An instant-read thermometer inserted into the center should read at least 160 degrees F (70 degrees C)",
        "Let rest 5 minutes before cutting"
    ],

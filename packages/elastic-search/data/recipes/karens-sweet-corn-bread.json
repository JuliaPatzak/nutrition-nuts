    "ingredients": [
        "\u00bd cup butter",
        "\u00bd cup white sugar",
        "2  eggs",
        "1 cup yellow cornmeal",
        "1\u2009\u00bd cups all-purpose flour",
        "2 teaspoons baking powder",
        "\u00bd teaspoon salt",
        "1\u2009\u00bd cups milk"
    ],
    "directions": [
        "Preheat oven to 375 degrees F (190 degrees C)",
        "Grease an 8-inch square pan",
        "Beat butter and sugar together in a bowl until creamy",
        "Add eggs; beat until combined",
        "Stir in cornmeal",
        "Sift flour, baking powder, and salt together in a separate bowl",
        "Stir flour mixture, alternating with milk, into butter mixture until well combined",
        "Pour batter into prepared pan",
        "Bake in the preheated oven until a toothpick inserted in the center of the bread comes out clean, 30 to 35 minutes"
    ],

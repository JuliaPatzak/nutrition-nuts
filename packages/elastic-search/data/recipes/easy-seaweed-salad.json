    "ingredients": [
        "2 ounces dried wakame seaweed",
        "\u00bc cup rice vinegar",
        "3  green onions, thinly sliced",
        "2 tablespoons soy sauce, or more to taste",
        "2 tablespoons sesame oil",
        "1 tablespoon Asian sweet chili sauce, or more to taste",
        "1 clove garlic, minced",
        "1 teaspoon grated fresh ginger, or more to taste",
        "1 packet stevia powder  (Optional)",
        "2 tablespoons toasted sesame seeds  (Optional)"
    ],
    "directions": [
        "Rinse wakame, place in a bowl, and cover with water",
        "Let soak until soft, 5 to 6 minutes",
        "Drain and squeeze dry; slice into thin strips",
        "Whisk rice vinegar, green onions, soy sauce, sesame oil, chili sauce, garlic, ginger, and stevia powder in a serving bowl",
        "Add wakame strips; toss until coated",
        "Sprinkle sesame seeds on top"
    ],

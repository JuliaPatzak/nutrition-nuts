    "ingredients": [
        "8 pounds potatoes, peeled and cubed",
        "1 small onion, chopped",
        "2 tablespoons butter",
        "2 cubes  chicken bouillon",
        "2 tablespoons dried parsley",
        "6 cups water",
        "2 cups milk",
        "\u00bd cup all-purpose flour"
    ],
    "directions": [
        "Place the potatoes, onion, butter, chicken bouillon cubes, parsley and water into a slow cooker",
        "Set on low and let cook for 6 to 8 hours",

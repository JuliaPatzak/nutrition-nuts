    "ingredients": [
        "6 slices bacon, chopped",
        "2 medium (2-1/2\" dia)s sweet onions, cut into thin strips",
        "2 tablespoons molasses",
        "\u00bc teaspoon salt",
        "\u00bc teaspoon pepper"
    ],
    "directions": [
        "Place bacon in a heavy skillet",
        "Cook over medium-high heat until crisp",
        "Remove bacon, reserving 1 tablespoon drippings in skillet",
        "Crumble bacon, and set aside",
        "Cook onions in reserved drippings for 15 minutes, or until onion is soft and caramel colored",
        "Stir in molasses, salt and pepper",
        "Place in a serving dish, and sprinkle with crumbled bacon"
    ],

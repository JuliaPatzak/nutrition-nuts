    "ingredients": [
        "1 pound ground beef",
        "1 cup chopped onion",
        "1 cup chopped green bell pepper",
        "1 tablespoon brown sugar",
        "1 tablespoon vinegar",
        "1 cup ketchup",
        "2 tablespoons prepared mustard",
        "\u00bd teaspoon ground cloves",
        "1 teaspoon salt",
        "4  hamburger buns, split"
    ],
    "directions": [
        "In a large skillet over medium heat, combine the ground beef, onion, and green pepper",
        "Cook until beef is browned, and drain off excess grease",
        "Stir in the brown sugar, vinegar, ketchup and mustard, and season with cloves and salt",
        "Simmer for 30 minutes on low",
        "Serve on hamburger buns"
    ],

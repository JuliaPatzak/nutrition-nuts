    "ingredients": [
        "1\u2009\u00bd pounds red potatoes, peeled",
        "1\u2009\u00bd cups mayonnaise",
        "1 cup chopped celery",
        "\u00bd cup chopped onion",
        "1 tablespoon white vinegar",
        "1 tablespoon prepared yellow mustard",
        "1 teaspoon salt",
        "\u00bc teaspoon ground black pepper",
        "4  hard-cooked eggs, chopped",
        "1 teaspoon paprika, or to taste  (Optional)"
    ],
    "directions": [
        "Place potatoes into a large pot and cover with salted water; bring to a boil",
        "Reduce heat to medium-low and simmer until tender, about 20 minutes",
        "Drain and let cool enough to handle, 10 to 15 minutes",
        "Cut into cubes",
        "Mix mayonnaise, celery, onion, vinegar, mustard, salt, and pepper together in a large bowl",
        "Add potatoes and mix to combine",
        "Stir in eggs and sprinkle with paprika"
    ],

    "ingredients": [
        "1 cup butter",
        "1 cup white sugar",
        "1  egg",
        "1 teaspoon vanilla extract",
        "2 tablespoons milk",
        "2\u2009\u00bd cups all-purpose flour",
        "\u00bd teaspoon baking soda",
        "\u00bc teaspoon salt"
    ],
    "directions": [
        "Preheat oven to 375 degrees F (190 degrees C)",
        "Cream together butter or margarine and  white sugar",
        "Add egg,  vanilla and  milk",
        "Mix well",
        "Gradually add flour, baking soda and salt",
        "Mix well",
        "Roll spoonfuls of dough in your hands and dip one end in colored sugar",
        "Place on baking sheets and press top down with a glass",
        "Bake for 12 minutes"

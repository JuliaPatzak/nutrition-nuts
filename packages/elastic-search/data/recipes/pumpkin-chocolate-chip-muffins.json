    "ingredients": [
        "1 (15 ounce) can pumpkin puree",
        "2 cups white sugar",
        "4  eggs",
        "1 cup coconut oil",
        "\u00bd cup olive oil",
        "3 cups whole wheat flour",
        "2 cups hemp hearts",
        "\u00bd cup wheat germ",
        "\u00bd cup ground flax seeds",
        "\u00bd cup brewers' yeast",
        "2 teaspoons baking soda",
        "2 teaspoons baking powder",
        "1\u2009\u00bd teaspoons ground cinnamon, or more to taste",
        "1 teaspoon salt",
        "1 (1.1 oz) package hemp protein powder",
        "2 cups dark chocolate chips",
        "\u00bc cup blackstrap molasses"
    ],
    "directions": [
        "Preheat the oven to 400 degrees F (200 degrees C)",
        "Line 36 muffin cups with paper liners",
        "Mix pumpkin, sugar, eggs, coconut oil, and olive oil together in a mixing bowl",
        "Combine flour, hemp, wheat germ, flax seeds, yeast, baking soda, baking powder, cinnamon, salt, and hemp powder in another bowl",
        "Blend into pumpkin mixture",
        "Stir in dark chocolate chips and molasses",
        "Fill the prepared muffins tins evenly with batter",
        "Bake in the preheated oven until a toothpick inserted into the center of a muffin comes out clean, 15 to 18 minutes"
    ],

    "ingredients": [
        "3 cups milk",
        "2\u2009\u00bd cups rolled oats",
        "1 cup Greek yogurt",
        "\u2153 cup honey",
        "\u2153 cup peanut butter",
        "\u2153 cup flaxseed meal",
        "1 tablespoon vanilla extract",
        "1 teaspoon salt",
        "1\u2009\u00bc cups dried cranberries",
        "1\u2009\u00bc cups chopped walnuts"
    ],
    "directions": [
        "Combine milk, oats, yogurt, honey, peanut butter, flaxseed meal, vanilla extract, and salt in the bowl of a stand mixer",
        "Mix on low speed until oats are soft, about 30 minutes",
        "Pour mixture into 10 half-pint jars",
        "Sprinkle 2 tablespoons cranberries and 2 tablespoons walnuts into each jar",
        "Refrigerate before serving, 1 to 10 days"
    ],

    "ingredients": [
        "2 teaspoons simple syrup",
        "1 teaspoon water",
        "2 dashes bitters",
        "1 cup ice cubes",
        "1 (1.5 fluid ounce) jigger bourbon whiskey",
        "1 slice orange",
        "1  maraschino cherry"
    ],
    "directions": [
        "Pour the simple syrup, water, and bitters into a whiskey glass",

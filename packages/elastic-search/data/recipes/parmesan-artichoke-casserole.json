    "ingredients": [
        "2 (10 ounce) cans artichoke hearts in water, drained",
        "8 ounces grated Parmesan cheese",
        "2 teaspoons garlic powder",
        "\u00bd cup mayonnaise",
        "2 tablespoons dried parsley",
        "1 pinch paprika"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Place artichoke hearts in a glass mixing bowl",
        "Mix in cheese, garlic powder, mayonnaise and parsley until well coated",
        "Spread mixture in 9x13 inch glass baking dish and sprinkle with paprika",
        "Bake in preheated 350 degrees F (175 degrees C) oven until bubbly, about 25 to 35 minutes",
        "Serve hot"
    ],

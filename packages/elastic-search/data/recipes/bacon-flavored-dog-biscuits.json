    "ingredients": [
        "2  eggs",
        "1 cup milk",
        "\u00bd cup water",
        "1 teaspoon salt",
        "10 tablespoons bacon fat, melted",
        "5 cups whole wheat flour"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Lightly grease a cookie sheet",
        "Beat eggs in a large bowl",
        "Stir in milk, water, salt, and bacon fat until well blended",
        "Gradually stir in flour to make a stiff dough",
        "Pinch off pieces of the dough and roll into 2 inch balls",
        "Place on prepared baking sheet",
        "Bake in preheated oven for 35 to 40 minutes",
        "Cool on racks",
        "Store in a covered container in the refrigerator"
    ],

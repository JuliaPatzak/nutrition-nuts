    "ingredients": [
        "2  eggs, beaten",
        "4 cups milk",
        "\u00bd cup white sugar",
        "\u00bd cup uncooked white rice",
        "1 tablespoon butter",
        "1 teaspoon vanilla extract",
        "\u00bd cup raisins  (Optional)",
        "\u215b teaspoon ground nutmeg"
    ],
    "directions": [
        "Preheat oven to 300 degrees F (150 degrees C)",
        "Grease a 2 quart baking dish",
        "Beat together the eggs and milk",
        "Stir in white sugar, uncooked rice, butter, vanilla extract, raisins, and nutmeg",
        "Pour into prepared pan",
        "Bake for 2 to 2 1/2 hours in the preheated oven",
        "Stir frequently during the first hour"

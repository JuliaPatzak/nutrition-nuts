    "ingredients": [
        "2 (32 ounce) jars dill pickles",
        "4 cups white sugar, divided",
        "\u00bd cup distilled white vinegar",
        "\u00bd cup water",
        "1 tablespoon pickling spice"
    ],
    "directions": [
        "Drain pickles and slice into 1/4 inch pieces",
        "Place in a medium bowl with 2 cups sugar",
        "Stir, cover and allow to stand at room temperature 6 to 8 hours, or overnight",
        "In a medium saucepan, mix together the remaining sugar, vinegar, water and pickling spice",
        "Bring to a boil",
        "Pour over the pickles",
        "Allow the mixture to stand at room temperature 6 to 8 hours, or overnight",
        "Transfer the pickles to sterile jars and store in the refrigerator",
        "Serve chilled"
    ],

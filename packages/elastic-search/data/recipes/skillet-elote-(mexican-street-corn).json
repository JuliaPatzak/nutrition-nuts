    "ingredients": [
        "2 tablespoons mayonnaise",
        "1 tablespoon lime juice",
        "1\u2009\u00bd teaspoons chili powder",
        "4 ears sweet corn, shucked and kernels cut off",
        "\u00bc cup chopped cilantro",
        "3 tablespoons cotija cheese"
    ],
    "directions": [
        "Whisk mayonnaise, lime juice, and chili powder together in a bowl until evenly combined",
        "Set aside",
        "Heat a large cast iron skillet over medium-high heat",
        "Add corn and cook for 5 minutes without stirring",
        "Toss around with a spatula and cook until corn has started to brown, about 3 minutes more",
        "Remove from heat",
        "Add reserved mayonnaise mixture and stir until evenly combined",
        "Add cilantro and stir again",
        "Transfer to serving plates and sprinkle cotija cheese over the top"
    ],

    "ingredients": [
        "\u00bd cup light mayonnaise",
        "2 teaspoons lime juice",
        "1 tablespoon finely chopped, seeded fresh jalapeno pepper, or more to taste",
        "1 (6 ounce) can water-packed, chunk light tuna, drained",
        "6 (7 inch) flour tortillas",
        "2 cups thinly sliced iceberg lettuce",
        "\u00bd cup shredded Mexican blend cheese",
        "1  plum tomato, seeded and diced, or more to taste",
        "1  avocado, diced  (Optional)",
        "1  green onion, diced  (Optional)",
        "2 tablespoons chopped fresh cilantro  (Optional)"
    ],
    "directions": [
        "Combine mayonnaise, lime juice, and jalapeno pepper in a medium bowl; mix well",
        "Stir in tuna",
        "Spread tuna mixture to within 1/4 inch of the edge of each tortilla",
        "Top with lettuce, cheese, and tomato; press lightly to flatten a bit",
        "Top with avocado, green onion, and cilantro",
        "Roll up each tortilla tightly or fold in half",
        "Serve immediately"
    ],

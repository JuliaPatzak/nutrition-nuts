    "ingredients": [
        "2\u2009\u00bd cups all-purpose flour",
        "2 teaspoons baking powder",
        "2 teaspoons baking soda",
        "1 teaspoon salt",
        "1 cup white sugar",
        "1 cup packed brown sugar",
        "1 cup butter, softened",
        "2  eggs",
        "1 teaspoon vanilla extract",
        "2 cups rolled oats",
        "2 cups flaked coconut"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Lightly grease baking sheets",
        "Sift the flour, baking powder, baking soda and salt together into a bowl",
        "Cream the butter and the sugar together",
        "Beat in the egg and vanilla",
        "Add the flour mixture and beat until smooth, about 2 minutes",
        "Stir in the oats and flaked coconut",
        "Shape dough into 1 inch balls and place on the prepared baking sheets",
        "Bake at 350 degrees F (175 degrees C) for 12  to 15 minutes"
    ],

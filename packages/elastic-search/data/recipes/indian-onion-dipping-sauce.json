    "ingredients": [
        "1  onion, chopped, or more to taste",
        "1\u2009\u00bd tablespoons lemon juice",
        "1 teaspoon ketchup, or more to taste  (Optional)",
        "\u00bd teaspoon cayenne pepper",
        "\u00bd teaspoon Hungarian paprika",
        "\u00bd teaspoon salt",
        "1 pinch white sugar  (Optional)"
    ],
    "directions": [
        "Place onion in a food processor and pulse until onion is minced",
        "Add lemon juice, ketchup, cayenne pepper, paprika, salt, and sugar and pulse until evenly combined",
        "Transfer sauce to an airtight container and refrigerate until flavors combine, 2 to 3 hours"

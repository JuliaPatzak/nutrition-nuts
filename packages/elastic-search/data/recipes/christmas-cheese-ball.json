    "ingredients": [
        "2 (8 ounce) packages cream cheese, softened",
        "2 cups shredded Cheddar cheese",
        "1 cup chopped green bell pepper, or to taste",
        "\u00bd large onion, chopped, or to taste",
        "1 (4 ounce) jar sliced pimento peppers",
        "2 teaspoons Worcestershire sauce",
        "1 teaspoon lemon juice",
        "1 pinch salt"
    ],
    "directions": [
        "Mix cream cheese and Cheddar cheese together in a bowl using your hands",
        "Add green bell pepper, onion, pimento peppers, Worcestershire sauce, lemon juice, and salt; mix well",
        "Shape mixture into a ball and place in a bowl",
        "Cover bowl with plastic wrap and refrigerate for 2 hours"
    ],

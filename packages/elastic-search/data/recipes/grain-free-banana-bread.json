    "ingredients": [
        "3  ripe bananas",
        "1 (15 ounce) can chickpeas (garbanzo beans), drained and rinsed",
        "\u00bd cup melted butter",
        "\u00bd cup chopped pitted dates",
        "\u00bd cup almond flour",
        "\u00bc cup honey",
        "4  eggs",
        "1 teaspoon baking soda",
        "1 teaspoon grated fresh ginger",
        "1 teaspoon ground cinnamon",
        "1 teaspoon vanilla extract",
        "\u00be teaspoon salt"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease 1 large loaf pan",
        "Blend bananas, chickpeas, butter, dates, almond flour, honey, eggs, baking soda, ginger, cinnamon, vanilla extract, and salt together in a blender or food processor until batter is smooth",
        "Pour batter into the prepared loaf pan",
        "Bake in the preheated oven until golden brown and a knife inserted in the center of the loaf comes out clean, about 1 hour",
        "Cool in pan for 15 minutes before slicing"
    ],

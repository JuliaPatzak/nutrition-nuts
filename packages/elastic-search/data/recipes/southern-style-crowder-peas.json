    "ingredients": [
        "4 slices bacon",
        "1 small onion, chopped",
        "1 small green bell pepper, chopped",
        "1 (16 ounce) package frozen crowder peas",
        "2 cups water",
        "1 tablespoon garlic powder",
        "1 pinch dried thyme",
        "1  crushed bay leaf",
        "\u00bd teaspoon seasoned salt to taste",
        "2 slices cooked ham, cubed",
        "1 tablespoon chopped fresh parsley"
    ],
    "directions": [
        "Place bacon in a skillet over medium-high heat, and cook until evenly brown",
        "Drain and crumble",
        "Mix onion and green bell pepper into the skillet, and cook until tender",
        "Stir in peas and water",
        "Season with garlic powder, thyme, bay leaf, and seasoned salt",
        "Bring to a boil",
        "Reduce heat to low, and simmer 30 to 40 minutes, until peas are tender",
        "Mix ham into the skillet",
        "Adjust seasonings to taste",
        "Continue cooking 5 minutes",
        "Stir in fresh parsley just before serving"
    ],

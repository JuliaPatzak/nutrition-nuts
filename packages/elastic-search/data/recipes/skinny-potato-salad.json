    "review_count": 1,
    "ingredients": [
        "4 cups new potatoes",
        "\u00bd cup plain Greek yogurt",
        "2 tablespoons mayonnaise",
        "2 tablespoons prepared brown mustard, or more to taste",
        "1 tablespoon cooking sherry",
        "1 tablespoon pickle juice",
        "\u00bd teaspoon salt, or to taste",
        "1 pinch ground black pepper",
        "1 stalk celery, chopped",
        "2  green onions, chopped",
        "2 tablespoons chopped dill pickles"
    ],
    "directions": [
        "Place potatoes into a large pot and cover with salted water; bring to a boil",
        "Reduce heat to medium-low and simmer until tender, about 20 minutes",
        "Drain and let cool",
        "Mix yogurt, mayonnaise, mustard, sherry, pickle juice, salt, and pepper together to make dressing",
        "Halve or quarter potatoes and place in a bowl",
        "Add celery, green onions, and pickles",
        "Spoon in dressing and toss to combine"
    ],

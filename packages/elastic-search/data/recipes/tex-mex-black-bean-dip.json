    "ingredients": [
        "1 (15 ounce) can black beans, rinsed and drained",
        "1 teaspoon vegetable oil",
        "\u00bd cup chopped onion",
        "2 cloves garlic, minced",
        "\u00bd cup fresh corn kernels",
        "\u00be cup chopped tomatoes",
        "\u00bd cup mild picante sauce",
        "1 teaspoon ground cumin",
        "1 teaspoon chili powder",
        "\u00bd cup shredded Monterey Jack cheese",
        "\u00bc cup chopped fresh cilantro",
        "1 tablespoon fresh lime juice"
    ],
    "directions": [
        "Place black beans in a medium size mixing bowl, partially mash beans -- beans should remain a little chunky",
        "In a medium size frying pan, heat oil over a medium heat",
        "Stir in onion and garlic and saute for 4 minutes",
        "Mix beans, corn, tomato, picante sauce, cumin, and chili powder into the frying pan; cook for 5 minutes or until thickened",
        "Remove the pan from the heat, mix in cheese, cilantro and lime juice; stir until cheese is melted"
    ],

    "ingredients": [
        "1 pound rotini pasta",
        "4  carrots, shredded",
        "1  green bell pepper, chopped",
        "1  onion, diced",
        "1 cup distilled white vinegar",
        "1 cup white sugar",
        "1 (14 ounce) can sweetened condensed milk",
        "2 cups mayonnaise",
        "1 teaspoon salt",
        "\u00bd teaspoon ground black pepper"
    ],
    "directions": [
        "Bring a large pot of lightly salted water to a boil",
        "Add pasta and cook for 8 to 10 minutes or until al dente; drain",
        "In large bowl, combine pasta, carrot, green pepper and onion",
        "In medium bowl, combine vinegar, sugar, condensed milk, mayonnaise, salt and pepper",
        "Toss salad with dressing and chill 4 hours in refrigerator before serving"

    "ingredients": [
        "3 tablespoons olive oil",
        "\u00bd cup chopped onion",
        "1 teaspoon minced garlic",
        "4 cups cubed fresh pumpkin",
        "\u00bd cup grated carrot",
        "2 tablespoons soy sauce",
        "1 teaspoon salt",
        "1 teaspoon ground black pepper",
        "2 teaspoons black sesame seeds"
    ],
    "directions": [
        "Heat the oil in a large skillet",
        "Add the onion and garlic; cook and stir until the onions begin to brown",
        "Stir in the pumpkin, carrot, soy sauce, salt, and pepper; cook another 5 to 7 minutes, until pumpkin is tender",
        "Sprinkle with sesame seeds before serving"
    ],

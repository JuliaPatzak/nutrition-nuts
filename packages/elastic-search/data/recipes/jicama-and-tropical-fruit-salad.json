    "ingredients": [
        "2 cups thinly sliced jicama",
        "1 cup roughly chopped mango",
        "1 cup roughly chopped pineapple",
        "4  kiwis - peeled, sliced, and quartered",
        "3 tablespoons freshly squeezed lime juice",
        "3 tablespoons chopped fresh mint leaves",
        "2 teaspoons honey",
        "1 teaspoon lime zest"
    ],
    "directions": [
        "Toss jicama, mango, pineapple, kiwis, lime juice, mint, honey, and lime zest together in a bowl",
        "Chill, covered, for at least 2 hours before serving"
    ],

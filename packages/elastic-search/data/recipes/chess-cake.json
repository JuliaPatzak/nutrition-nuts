    "ingredients": [
        "1 (18.25 ounce) package yellow cake mix",
        "2  eggs",
        "\u00bd cup butter, melted",
        "1 (8 ounce) package cream cheese, softened",
        "\u00bd cup butter, melted",
        "2  eggs",
        "4 cups confectioners' sugar"
    ],
    "directions": [
        "Preheat oven to 325 degrees F (165 degrees C)",
        "Grease and flour one 9 x 13 inch pan",
        "In a large bowl, mix together cake mix, 2 eggs, and 1/2 cup melted butter",
        "Put into prepared pan",
        "In another bowl, Beat cream cheese until smooth",
        "Blend in 1/2 cup melted butter and 2 eggs",
        "Gradually beat in the confectioner's sugar",
        "Pour over the crust mixture already in the pan",
        "Bake for 1 hour",
        "Cool"
    ],

    "ingredients": [
        "1 cup frozen corn",
        "1 tablespoon salsa",
        "1 tablespoon crema con sal (salted Mexican-style sour cream)",
        "2 tablespoons shredded asadero cheese, or to taste",
        "1 teaspoon minced cilantro",
        "1 pinch ground black pepper"
    ],
    "directions": [
        "Heat a skillet over medium-high heat",
        "Add corn, salsa, and crema con sal",
        "Cook until heated through, 3 to 4 minutes",
        "Remove from heat",
        "Sprinkle with cheese, cilantro, and black pepper",
        "Serve immediately"
    ],

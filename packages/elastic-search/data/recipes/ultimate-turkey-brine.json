    "ingredients": [
        "1 gallon water",
        "2 cups kosher salt",
        "1\u2009\u00bd cups dark brown sugar",
        "12  whole cloves",
        "3  bay leaves",
        "1 tablespoon whole black peppercorns",
        "2\u2009\u00bd teaspoons dried rosemary",
        "2\u2009\u00bd teaspoons dried thyme"
    ],
    "directions": [
        "Bring water to a boil in a large stockpot",
        "Stir salt and sugar into the water until dissolved; bring water again to a boil",
        "Add cloves, bay leaves, peppercorns, rosemary, and thyme to the water, reduce heat to medium-low, and cook mixture at a simmer for about 20 minutes",
        "Refrigerate brine until completely cooled"
    ],

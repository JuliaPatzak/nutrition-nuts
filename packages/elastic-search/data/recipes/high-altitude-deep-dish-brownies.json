    "ingredients": [
        "\u215e cup all-purpose flour",
        "\u00bd cup unsweetened cocoa powder",
        "\u00bd teaspoon baking powder",
        "\u00bd teaspoon salt",
        "\u00be cup butter, melted",
        "1\u2009\u00bd cups white sugar",
        "1\u2009\u00bd teaspoons vanilla extract",
        "3  eggs",

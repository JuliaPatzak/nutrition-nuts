    "ingredients": [
        "1 (8 ounce) can tuna",
        "5 tablespoons mayonnaise",
        "4 tablespoons salsa",
        "6 tablespoons vegetable oil",
        "4 (6 inch) corn tortillas",
        "1 large tomato, diced",
        "\u00bd cup chopped fresh spinach",
        "\u00bd cup diced onion",
        "4 tablespoons shredded Cheddar cheese",
        "1  avocado, sliced"
    ],
    "directions": [
        "Mix tuna, mayonnaise, and salsa together in a bowl",
        "Set aside",
        "Heat vegetable oil over medium-high heat in a frying pan",
        "Add tortillas in batches and fry until golden and slightly crisp, 2 to 3 minutes",
        "Spread tuna mixture over tortillas evenly",
        "Top with tomato, spinach, and onions",
        "Sprinkle with Cheddar cheese and top with avocado slices"
    ],

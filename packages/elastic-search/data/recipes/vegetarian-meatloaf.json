    "ingredients": [
        "1 (12 ounce) bottle barbeque sauce",
        "1 (12 ounce) package vegetarian burger crumbles",
        "1  green bell pepper, chopped",
        "\u2153 cup minced onion",
        "1 clove garlic, minced",
        "\u00bd cup soft bread crumbs",
        "3 tablespoons Parmesan cheese",
        "1  egg, beaten",
        "\u00bc teaspoon dried thyme",
        "\u00bc teaspoon dried basil",
        "\u00bc teaspoon parsley flakes",
        "salt and pepper to taste"
    ],
    "directions": [
        "Preheat oven to 325 degrees F (165 degrees C)",
        "Lightly grease a 5x9 inch loaf pan",
        "In a bowl, mix 1/2 the barbeque sauce with the vegetarian burger crumbles, green bell pepper, onion, garlic, bread crumbs, Parmesan cheese, and egg",
        "Season with thyme, basil, parsley, salt, and pepper",
        "Transfer to the loaf pan",
        "Bake 45 minutes in the preheated oven",
        "Pour remaining barbeque sauce over the loaf, and continue baking 15 minutes, or until loaf is set"
    ],

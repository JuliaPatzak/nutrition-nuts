    "ingredients": [
        "10 ounces elbow macaroni",
        "1 cup mayonnaise",
        "1 cup sour cream",
        "\u00bd cup cubed Cheddar cheese",
        "10 slices bread-and-butter pickles, diced",
        "1  carrot, diced",
        "1 rib celery, diced",
        "\u00bd  green bell pepper, diced",
        "2 tablespoons diced red onion",
        "1 tablespoon diced banana peppers",
        "1 tablespoon salad seasoning mix (such as McCormick\u00ae Salad Supreme Seasoning)",
        "1 teaspoon Dijon mustard",
        "\u00bc teaspoon salt",
        "\u00bc teaspoon white sugar"
    ],
    "directions": [
        "Bring a large pot of lightly salted water to a boil",
        "Cook elbow macaroni in the boiling water, stirring occasionally, until tender yet firm to the bite, about 8 minutes",
        "Combine mayonnaise, sour cream, Cheddar cheese, pickles, carrot, celery, bell pepper, onion, banana peppers, salad seasoning, mustard, salt, and sugar in a large bowl",
        "Mix well",
        "Drain pasta and let cool for about 10 minutes",
        "Toss pasta into the dressing; chill before serving"
    ],

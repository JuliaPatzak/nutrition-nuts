    "ingredients": [
        "1 pound ground lamb",
        "2 slices bread, torn into small pieces",
        "\u00bc cup yogurt",
        "\u00bc cup raisins",
        "\u00bc cup fresh cilantro",
        "1 clove garlic, minced",
        "\u00bc teaspoon dried oregano",
        "\u00bc teaspoon dried basil",
        "\u215b teaspoon salt",
        "\u215b teaspoon ground black pepper"
    ],
    "directions": [
        "Preheat the oven to 375 degrees F (190 degrees C)",
        "Mix lamb, bread, yogurt, raisins, cilantro, garlic, oregano, basil, salt, and pepper together in a bowl",

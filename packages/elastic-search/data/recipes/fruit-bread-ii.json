    "ingredients": [
        "2 cups all-purpose flour",
        "\u00be cup white sugar",
        "3\u2009\u00bd teaspoons baking powder",
        "\u00be teaspoon salt",
        "\u00bc cup candied pineapple, diced",
        "\u00bd cup currants",
        "\u00bd cup chopped candied cherries",
        "2  eggs",
        "1 cup milk",
        "\u00bc cup vegetable oil"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease one 9x5x3 inch loaf pan",
        "In large bowl measure in flour, sugar, baking powder, salt, candied pineapple, raisins or currants, and cherries",
        "Stir thoroughly",
        "Make a well in center",
        "In small bowl beat eggs until frothy",
        "Mix in milk and oil",
        "Pour into well",
        "Stir just to moisten",
        "Pour into greased 9x5x3 inch loaf pan",
        "Bake at 350 degrees F (175 degrees C) for 1 hour",
        "Let cool in pan 10 minutes, then remove"

    "ingredients": [
        "cooking spray",
        "8 cups shredded cabbage",
        "1  apple, sliced",
        "1  white onion, sliced",
        "2 tablespoons olive oil",
        "1\u2009\u00bd tablespoons white vinegar",
        "1 teaspoon salt",
        "\u00bd teaspoon ground black pepper",
        "8 links chicken sausage"
    ],
    "directions": [
        "Preheat oven to 425 degrees F (220 degrees C)",
        "Grease a rimmed baking sheet with cooking spray",
        "Combine cabbage, apple, onion, olive oil, vinegar, salt, and pepper; toss until well coated",
        "Place in a single layer on the prepared baking sheet",
        "Bake in the preheated oven until slightly tender, about 20 minutes",
        "Toss cabbage mixture and place sausages on top",
        "Bake until sausages are crispy and cabbage has caramelized, about 10 minutes more"
    ],

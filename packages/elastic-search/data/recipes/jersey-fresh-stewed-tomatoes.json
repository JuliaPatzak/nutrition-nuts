    "ingredients": [
        "6 large tomatoes - peeled, cored, and chopped",
        "\u00be cup chopped green bell pepper",
        "\u00bd cup chopped sweet onion",
        "\u00bd cup chopped celery",
        "1 teaspoon white sugar",
        "\u00bd teaspoon salt",
        "\u00bc teaspoon dried oregano",
        "\u00bc teaspoon dried basil",
        "\u215b teaspoon black pepper",
        "1 tablespoon water",
        "1\u2009\u00bd teaspoons cornstarch"
    ],
    "directions": [
        "Combine tomatoes, green bell pepper, onion, and celery in a large skillet over medium heat; cook and stir until fragrant, about 10 minutes",
        "Reduce heat and cook until green bell pepper and onion are soft, about 5 minutes more",
        "Stir in sugar, salt, oregano, basil, and pepper",
        "Whisk water and cornstarch together in a small bowl",
        "Stir into the skillet",
        "Simmer tomato mixture until thickened, about 5 minutes"

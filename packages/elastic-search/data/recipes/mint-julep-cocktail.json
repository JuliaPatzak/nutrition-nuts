    "ingredients": [
        "8  mint leaves",
        "1 teaspoon simple syrup",
        "2\u2009\u00bd fluid ounces bourbon whiskey",
        "crushed ice, divided",
        "1 sprig fresh mint for garnish"
    ],
    "directions": [
        "Chill a julep cup in the freezer for about 5 minutes",
        "Place mint leaves and simple syrup in the chilled julep cup; mash well with a cocktail muddler",
        "Pour in bourbon and 1/2 cup crushed ice",
        "Stir until chilled and top with remaining 1/2 cup ice",
        "Garnish with fresh mint"
    ],

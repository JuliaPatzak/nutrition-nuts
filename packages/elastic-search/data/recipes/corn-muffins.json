    "ingredients": [
        "1 cup all-purpose flour",
        "1 cup yellow cornmeal",
        "\u00bd cup white sugar",
        "2 teaspoons baking powder",
        "1 teaspoon salt",
        "1 cup buttermilk",
        "\u00bd cup butter, melted",
        "1  egg, beaten"
    ],
    "directions": [
        "Preheat oven to 400 degrees F (200 degrees C)",
        "Grease 12 muffin cups",
        "Mix flour, cornmeal, sugar, baking powder, and salt in a large bowl",
        "Add buttermilk, butter, and egg; mix well",
        "Spoon the batter into the prepared muffin cups",
        "Bake in the preheated oven until a toothpick inserted into the center comes out clean, 12 to 15 minutes",
        "Cool in the pans 10 to 15 minutes before removing from pan to cool completely on a wire rack"
    ],

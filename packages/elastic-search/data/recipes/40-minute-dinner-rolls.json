    "ingredients": [
        "2 tablespoons active dry yeast",
        "1 cup water",
        "cooking spray",
        "1 large egg",
        "3 cups all-purpose flour"
    ],
    "directions": [
        "Mix water and yeast together in a large bowl",
        "Let sit for 5 minutes",
        "Preheat the oven to 425 degrees F (220 degrees C)",
        "Spray a cookie sheet with cooking spray",
        "Stir in egg",
        "Add flour slowly until a dough forms",
        "Roll dough onto a floured surface and knead until round and elastic",
        "Cut dough into 12 pieces and place on the prepared cookie sheet",
        "Cover with aluminum foil and let rest for 10 minutes",
        "Bake in the preheated oven until golden brown, 8 to 12 minutes"
    ],

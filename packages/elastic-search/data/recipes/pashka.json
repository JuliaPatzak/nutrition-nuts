    "ingredients": [
        "2 (8 ounce) packages cream cheese, softened",
        "1 (8 ounce) container cottage cheese",
        "1 (8 ounce) container sour cream",
        "\u00bc cup butter, melted",
        "1 cup white sugar",
        "\u00bc teaspoon lemon extract",
        "1 teaspoon vanilla extract",
        "\u00bd cup finely chopped blanched almonds",
        "\u00bd cup currants"
    ],
    "directions": [
        "In a medium bowl, thoroughly blend cream cheese, cottage cheese, sour cream and butter",
        "Gradually fold in sugar, lemon extract, vanilla extract, almonds and currants",
        "Transfer the mixture to a shallow medium dish",
        "Cover with a cloth, and chill in the refrigerator 8 hours, or overnight"
    ],

    "review_count": 19,
    "ingredients": [
        "1 (15 ounce) can chili",
        "1 (8 ounce) package cream cheese, cubed",
        "2 (8 ounce) packages shredded mozzarella cheese",
        "garlic powder to taste",
        "ground black pepper to taste"
    ],
    "directions": [
        "Preheat the broiler",
        "In a shallow, medium baking dish, spread the chili and mix in cream cheese",
        "Microwave on high 1 minute, or until cheese is melted and creamy",
        "Stir in 1/2 the mozzarella cheese, garlic powder and pepper",
        "Microwave on high 1 minute, or until melted",
        "Top the mixture with remaining mozzarella cheese",
        "Broil 5 minutes, or until cheese is bubbly and lightly browned"
    ],

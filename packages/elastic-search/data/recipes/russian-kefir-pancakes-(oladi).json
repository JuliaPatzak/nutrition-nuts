    "ingredients": [
        "1 cup all-purpose flour",
        "3 tablespoons all-purpose flour",
        "\u00bd cup plain kefir",
        "1  egg, beaten",
        "1 tablespoon white sugar",
        "\u2153 teaspoon baking soda",
        "1 pinch salt",
        "1 tablespoon vegetable oil"
    ],
    "directions": [
        "Whisk 1 cup plus 3 tablespoons flour, kefir, egg, sugar, baking soda, and salt together in a large bowl to make a batter",
        "Heat a nonstick skillet over medium-low heat",
        "Heat oil in the skillet",
        "Pour 1 to 2 tablespoons of the batter into the skillet for each pancake",
        "Cook until bubbles form on the surface, 2 to 3 minutes",
        "Flip and cook until browned on the other side, 2 to 3 minutes more",
        "Repeat with remaining batter"
    ],

    "ingredients": [
        "1 tablespoon olive oil",
        "1 medium onion, 1/2-inch dice",
        "1 clove garlic, minced",
        "1 tablespoon tomato paste",
        "1 (14 ounce) package Hillshire Farm\u00ae Smoked Sausage, cut into 1/4-inch slices",
        "1 (28 ounce) can diced tomatoes",
        "4 cups chicken broth",
        "1 (9 ounce) package fresh cheese tortellini",
        "1 teaspoon salt",
        "\u00bc teaspoon pepper",
        "2 cups baby spinach",
        "1 tablespoon Parmesan cheese"
    ],
    "directions": [
        "Heat oil in a large pot over medium heat",
        "Saute onion and garlic in oil until soft, about 5 minutes",
        "Stir in the tomato paste and smoked sausage and cook for an additional 3 to 4 minutes",
        "Add the diced tomatoes, broth, tortellini, salt and pepper and bring to a boil",
        "Reduce heat and simmer gently for 5 minutes, then stir in spinach and cook for an additional 3 to 4 minutes",
        "Serve with a sprinkle of Parmesan cheese, if desired"
    ],

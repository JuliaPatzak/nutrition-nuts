    "ingredients": [
        "2 cups all-purpose flour",
        "1\u2009\u00bd cups white sugar",
        "2 tablespoons wheat germ",
        "2 tablespoons oat bran",
        "1 teaspoon baking soda",
        "\u00bd teaspoon salt",
        "2  eggs",
        "1 (14 ounce) can crushed pineapple in juice",
        "\u00bd cup shredded coconut  (Optional)"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Line 18 muffin cups with paper liners",
        "Mix flour, sugar, wheat germ, oat bran, baking soda, and salt together in a large mixing bowl",
        "Add eggs and pineapple; stir until just combined",
        "Fold in coconut",
        "Pour batter into lined muffin cups to about 2/3 full",
        "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 30 minutes"
    ],

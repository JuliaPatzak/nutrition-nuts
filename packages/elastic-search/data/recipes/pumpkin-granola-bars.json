    "ingredients": [
        "\u00be cup pumpkin puree",
        "1  egg",
        "\u00bc cup room-temperature butter",
        "\u00bc cup honey",
        "3 tablespoons molasses",
        "2 cups rolled oats",
        "\u00bd cup sunflower seeds",
        "\u00bc cup wheat germ",
        "2 tablespoons shredded unsweetened coconut",
        "2 teaspoons ground flax  (Optional)",
        "1 tablespoon grated orange zest",
        "\u00bd teaspoon ground cinnamon"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Lightly grease a jelly roll pan",
        "Beat pumpkin puree, egg, butter, honey, and molasses together in a large bowl",
        "Stir rolled oats, sunflower seeds, wheat germ, coconut, ground flax, orange zest, and cinnamon into the pumpkin mixture; spread into the prepared pan",
        "Bake in preheated oven until golden brown, about 40 minutes",
        "Cut into bars while still warm"

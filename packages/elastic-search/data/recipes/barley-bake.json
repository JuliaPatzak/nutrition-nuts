    "ingredients": [
        "\u00bc cup butter",
        "1  medium onion, diced",
        "1 cup uncooked pearl barley",
        "\u00bd cup pine nuts",
        "2  green onions, thinly sliced",
        "\u00bd cup sliced fresh mushrooms",
        "\u00bd cup chopped fresh parsley",
        "\u00bc teaspoon salt",
        "\u215b teaspoon pepper",
        "2 (14.5 ounce) cans vegetable broth"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Melt butter in a skillet over medium-high heat",
        "Stir in onion, barley, and pine nuts",
        "Cook and stir until barley is lightly browned",
        "Mix in green onions, mushrooms, and parsley",
        "Season with salt and pepper",
        "Transfer the mixture to a 2 quart casserole dish, and stir in the vegetable broth",
        "Bake 1 hour and 15 minutes in the preheated oven, or until liquid has been absorbed and barley is tender"

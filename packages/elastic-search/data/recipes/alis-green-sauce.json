    "ingredients": [
        "3  avocados, peeled and pitted",
        "3  green tomatoes",
        "4  fresh tomatillos, husks removed",
        "3 cloves garlic, peeled",
        "2  jalapeno peppers, seeded and halved",
        "5 sprigs fresh cilantro",
        "1\u2009\u00bd cups sour cream",
        "salt to taste"
    ],
    "directions": [
        "Place tomatoes, tomatillos, garlic, and jalapenos in a saucepan, and add enough water to cover",
        "Bring to a boil, and cook for 15 minutes, or until vegetables are tender",
        "Remove from heat, drain, and allow to cool",
        "Place avocados, the cooked vegetables, sour cream, and cilantro in a blender or food processor, and blend until smooth",
        "Season to taste with salt",
        "Cover, and refrigerate until ready to serve"
    ],

    "ingredients": [
        "1 pound crabmeat",
        "\u00bd cup mayonnaise",
        "1 tablespoon fresh lemon juice",
        "1 teaspoon Worcestershire sauce",
        "\u00bd teaspoon Old Bay Seasoning TM",
        "\u00bd teaspoon salt",
        "\u00bd teaspoon cayenne pepper",
        "\u00bd teaspoon dry mustard",
        "1  egg, beaten",
        "\u00bc cup fresh bread crumbs",
        "1 teaspoon paprika",
        "3 tablespoons butter"
    ],
    "directions": [
        "Preheat oven to 400 degrees F (200 degrees C)",
        "In a medium bowl, combine crab meat, mayonnaise, lemon juice, Worcestershire sauce,   Seasoning, salt, cayenne pepper, dry mustard, and beaten egg",
        "",
        "Mix thoroughly",
        "In an 8\" pie dish, spread mixture and lightly coat the entire dish with bread crumbs",
        "Then sprinkle the top with paprika",
        "Dot the dish with the butter",
        "Bake in a preheated oven for 20 minutes"
    ],

    "ingredients": [
        "4 cups all-purpose flour",
        "1 teaspoon salt",
        "2 teaspoons vegetable oil",
        "\u00bc teaspoon baking powder",
        "1 cup warm water",
        "1  egg, beaten"
    ],
    "directions": [
        "In a large bowl mix together the flour, salt, and baking powder",
        "Make a well in the center",
        "In a separate bowl mix together the vegetable oil, warm water, and beaten egg",
        "Pour into the well of the dry ingredients",
        "Knead dough for 8 to 10 minutes",
        "Cover dough and let rest for 2 hours",
        "Roll out and fill as desired"

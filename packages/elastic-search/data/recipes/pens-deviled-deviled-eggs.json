    "ingredients": [
        "12  eggs",
        "1 (4.5 ounce) can deviled ham",
        "1 tablespoon spicy brown mustard",
        "\u00bc cup mayonnaise",
        "1 pinch black pepper",
        "24 slices black olives",
        "Paprika for sprinkling"
    ],
    "directions": [
        "Place egg in a saucepan and cover with cold water",
        "Bring water to a boil and immediately remove from heat",
        "Cover and let eggs stand in hot water for 10 to 12 minutes",
        "Remove from hot water, cool, and peel",
        "Cut eggs in half lengthwise, place the yolks into a mixing bowl, and set the whites aside",
        "Mash the yolks with the deviled ham, mustard, mayonnaise, and black pepper",
        "Spoon or pipe filling into egg white halves",
        "Garnish each deviled egg with an olive slice, then sprinkle with paprika"
    ],

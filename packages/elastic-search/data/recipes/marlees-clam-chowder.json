    "ingredients": [
        "8 slices bacon",
        "1 cup chopped onion",
        "1 cup chopped celery",
        "7 cups clam juice",
        "3 (28 ounce) cans stewed tomatoes",
        "5 tablespoons dried thyme",
        "2 (6.5 ounce) cans minced clams"
    ],
    "directions": [
        "In a large pot over medium heat, cook bacon until crisp",
        "Remove and crumble",
        "Return to pot with onion and celery and cook until onion is translucent",
        "Stir in clam juice and tomatoes",
        "Season with thyme and pour in clams",
        "Simmer 45 minutes, until flavors are well blended"
    ],

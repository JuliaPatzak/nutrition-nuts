    "ingredients": [
        "2 (8 ounce) packages cream cheese, softened",
        "4 tablespoons mayonnaise",
        "2 cups shredded Cheddar cheese",
        "2 (6 ounce) cans crabmeat",
        "1\u2009\u00bd tablespoons fresh lemon juice",
        "2 teaspoons hot sauce",
        "2 tablespoons Worcestershire sauce",
        "paprika, for garnish"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "In a medium bowl, mix the cream cheese, mayonnaise, Cheddar cheese, crabmeat, lemon juice, hot sauce and Worcestershire sauce",
        "Transfer to a shallow 9x13 inch baking dish",
        "Garnish with paprika",
        "Bake in the preheated oven 30 minutes, or until golden brown and bubbly"
    ],

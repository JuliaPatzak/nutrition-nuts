    "ingredients": [
        "1 (8 ounce) can pineapple chunks, drained",
        "1 cup cubed, seeded watermelon",
        "1  mango, peeled and cubed",
        "1  green apple, cored and sliced",
        "\u00bc cup lime juice",
        "2 tablespoons honey, or more to taste",
        "1  jalapeno pepper, seeded and chopped",
        "2 teaspoons chili-lime seasoning (such as Tajin\u00ae)"
    ],
    "directions": [
        "Mix pineapple, watermelon, mango, and green apple in a large bowl",
        "Blend lime juice, honey, and jalapeno in a blender until smooth",
        "Pour jalapeno dressing into the large bowl and mix thoroughly",
        "Sprinkle chili-lime seasoning on top",
        "Chill for 1 to 2 hours before serving"

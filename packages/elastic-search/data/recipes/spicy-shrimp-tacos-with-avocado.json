    "review_count": 0,
    "ingredients": [
        "1 pound uncooked medium shrimp, peeled and deveined",
        "3 tablespoons olive oil, divided",
        "1 clove garlic, minced",
        "\u00bd teaspoon chili powder",
        "\u00bd teaspoon salt",
        "\u00bc teaspoon cayenne pepper",
        "2 tablespoons lime juice",
        "16 (5 inch) corn tortillas",
        "2 cups tomatillo salsa",
        "1  avocado - peeled, pitted, and chopped",
        "\u00bd cup sour cream",
        "2 tablespoons chopped fresh cilantro"
    ],
    "directions": [
        "Combine shrimp, 1 tablespoon olive oil, garlic, chili powder, salt, and cayenne pepper in a bowl; toss until well coated",
        "Cover and refrigerate for 30 minutes",
        "Heat remaining 2 tablespoons olive oil in a skillet over medium-high heat",
        "Place shrimp mixture in the skillet and cook until pink and cooked through, about 5 minutes",
        "Sprinkle with lime juice",
        "Heat tortillas on an ungreased skillet over medium-high heat for 10 to 15 seconds each",
        "Turn and heat for another 5 to 10 seconds",
        "Fill the heated tortillas with cooked shrimp",
        "Top with tomatillo salsa, avocado, and sour cream",
        "Sprinkle with chopped cilantro"
    ],

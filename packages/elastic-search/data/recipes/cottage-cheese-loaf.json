    "ingredients": [
        "1 cup cottage cheese",
        "1 cup packed brown sugar",
        "3  eggs",
        "1\u2009\u00be cups all-purpose flour",
        "2 teaspoons baking powder",
        "\u00bd teaspoon baking soda",
        "\u00bd teaspoon salt",
        "\u00bd cup chopped walnuts",
        "\u00bd cup candied mixed fruit peel"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease one 9x5x3 inch loaf pan",
        "Smooth cottage cheese in a blender or force through strainer",
        "Put into bowl",
        "Beat in sugar",
        "Add eggs one at a time, beating well after each addition",
        "Set aside",
        "Measure flour, baking powder, soda, salt, walnuts, and mixed peel into a second bowl",
        "Mix together well",
        "Pour into first bowl",
        "Stir to moisten",
        "Scrape into greased 9x5x3 inch loaf pan",
        "Bake at 350 degrees F (175 degrees C) for about 1 hour",
        "Cool for 10 minutes before turning out to finish cooling on rack",
        "Wrap well",
        "Serve with butter"
    ],

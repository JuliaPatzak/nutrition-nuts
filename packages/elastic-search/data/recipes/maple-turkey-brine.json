    "ingredients": [
        "4 quarts water, divided",
        "2 cups dark brown sugar",
        "1 cup soy sauce",
        "1 cup maple syrup",
        "\u00be cup sea salt",
        "8 cloves whole garlic cloves, peeled",
        "6  bay leaves",
        "3  large fresh thyme sprigs",
        "2 teaspoons whole black peppercorns",
        "1 cup sour mash whiskey"
    ],
    "directions": [
        "Place 2 quarts of water in a large pot over medium heat, and stir in brown sugar, soy sauce, maple syrup, sea salt, garlic cloves, bay leaves, thyme sprigs, peppercorns, and whiskey",
        "Stir to dissolve brown sugar and salt; bring to a boil",
        "Remove from heat, and stir in remaining 2 quarts of water",
        "Allow brine to cool completely before using"

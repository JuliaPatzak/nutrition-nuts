    "ingredients": [
        "\u00bc cup vegetable oil",
        "2 tablespoons all-purpose flour",
        "2 tablespoons chili powder",
        "1 (8 ounce) can tomato sauce",
        "1 cup water, or more if needed",
        "\u00bc teaspoon cumin",
        "\u00bd teaspoon garlic powder",
        "\u00bd teaspoon onion powder",
        "2 tablespoons brown sugar",
        "salt to taste"
    ],
    "directions": [
        "Heat oil in a skillet over medium heat",
        "Stir flour and chili powder into the oil; cook and stir until smooth",
        "Gradually stir tomato sauce, water, cumin, garlic powder, onion powder, and brown sugar into the flour mixture, respectively",
        "Reduce heat to low and cook until thickened, about 10 minutes"
    ],

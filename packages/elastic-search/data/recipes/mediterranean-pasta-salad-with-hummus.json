    "ingredients": [
        "1 (16 ounce) package bow-tie pasta",
        "1  English cucumber, peeled and diced",
        "2 medium tomatoes, seeded and diced",
        "\u00bc cup diced red onion",
        "1 teaspoon minced garlic",
        "1 cup hummus, or as needed",
        "4 leaves fresh basil, finely chopped, or more to taste",
        "salt and ground black pepper to taste",
        "\u00bd cup feta cheese"
    ],
    "directions": [
        "Bring a large pot of lightly salted water to a boil",
        "Cook bow-tie pasta at a boil, stirring occasionally, until tender yet firm to the bite, about 12 minutes",
        "Drain and refrigerate until cool to the touch, about 15 minutes",
        "Combine cucumber, tomatoes, red onion, and garlic in a medium bowl",
        "Add cooled pasta",
        "Toss with hummus",
        "Gently mix in basil",
        "Season with salt and pepper",
        "Adjust the amount of hummus based on how wet or dry the salad seems",
        "Garnish with feta cheese"
    ],

    "ingredients": [
        "Reynolds Wrap\u00ae Heavy Duty Aluminum Foil",
        "1 (10 ounce) jar sweet and sour sauce",
        "1 teaspoon garlic salt",
        "\u00bd teaspoon crushed red pepper flakes, or to taste",
        "2 pounds frozen cooked meatballs, thawed",
        "1 medium red bell pepper, cubed",
        "1 (20 ounce) can pineapple chunks, drained"
    ],
    "directions": [
        "Preheat oven to 450 degrees F",
        "Line a 15x10x1-inch baking pan with Reynolds Wrap\u00ae Heavy Duty Aluminum Foil; set aside",
        "Combine sweet and sour sauce, garlic salt and red pepper flakes in a large bowl",
        "Add meatballs, bell pepper and pineapple",
        "Stir to coat evenly",
        "Arrange in even layer in foil-lined pan",
        "Bake 25 minutes or until meatballs are hot; stirring once during baking",
        "Spoon sauce over meatballs",
        "Serve warm"
    ],

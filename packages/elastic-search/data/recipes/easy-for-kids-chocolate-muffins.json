    "ingredients": [
        "\u00bd cup butter",
        "\u00bd cup superfine sugar",
        "2  eggs",
        "1 cup self-rising flour",
        "2 teaspoons cocoa powder",
        "1 teaspoon baking soda"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Grease 6 cups in a muffin pan",
        "Cream together butter and sugar in a mixing bowl",
        "Mix in eggs",
        "Add flour, cocoa powder, and baking soda and blend thoroughly",
        "Spoon batter into muffin cups",
        "Bake 15 to 20 minutes, or until a toothpick inserted into the center comes out clean",
        "Cool and decorate"
    ],

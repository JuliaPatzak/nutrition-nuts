    "ingredients": [
        "\u00bd teaspoon baking soda",
        "2 cups persimmon pulp",
        "2\u2009\u00bd cups white sugar",
        "2  eggs, beaten",
        "2 cups all-purpose flour",
        "2 teaspoons baking powder",
        "\u00bd teaspoon ground cinnamon",
        "\u00bc teaspoon vanilla extract",
        "1 pinch salt",
        "2\u2009\u00bd cups milk",
        "4 tablespoons melted butter"
    ],
    "directions": [
        "Preheat oven to 325 degrees F (165 degrees C)",
        "Grease a 9x13-inch baking dish",
        "In a mixing bowl, combine persimmon pulp, baking soda, sugar and eggs",
        "Mix well",
        "Add flour, baking powder, cinnamon, vanilla, salt, milk and melted butter",
        "Stir to combine",
        "Pour into baking pan and bake in preheated oven for 55 minutes",
        "The pudding will rise but will fall when removed from oven"
    ],

    "ingredients": [
        "5 tablespoons vegetable oil",
        "1  onion, chopped",
        "1 tablespoon ground cumin",
        "1 teaspoon ground turmeric",
        "1 tablespoon ground coriander",
        "1 teaspoon chili powder",
        "\u00bd teaspoon salt",
        "6  potatoes, diced",
        "1 (10 ounce) package frozen mixed peas and carrots",
        "3 tablespoons water, or as needed"
    ],
    "directions": [
        "Heat the oil in a large skillet over medium-high heat",
        "Add the onion, and season with cumin, turmeric, coriander, chili powder and salt",
        "Cook and stir until onion is browned",
        "Reduce the heat to medium, and stir in the potatoes, carrots and peas",
        "Stir to coat, then cover and cook until potatoes are tender, about 20 minutes",
        "Stir occasionally, and add water if necessary to keep food from sticking to the pan"
    ],

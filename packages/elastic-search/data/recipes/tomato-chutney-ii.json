    "ingredients": [
        "2 cups chopped tomatoes",
        "2 teaspoons ginger",
        "1 teaspoon minced garlic",
        "2 tablespoons chopped cilantro",
        "2 teaspoons chili powder",
        "1 tablespoon white sugar",
        "salt to taste"
    ],
    "directions": [
        "In a saucepan over medium heat, combine the tomatoes, ginger, garlic, cilantro, chili powder sugar and salt",
        "Simmer over medium heat, stirring occasionally, until thick and saucy, about 10 minutes"

    "ingredients": [
        "1 cup milk",
        "1 package McCormick\u00ae Pork Gravy Mix",
        "1 (8 ounce) package whipped cream cheese",
        "\u00bc cup mayonnaise",
        "1 tablespoon yellow mustard",
        "1 cup cubed ham",
        "\u00bd cup shredded Swiss cheese",
        "1 gram Pickle chips"
    ],
    "directions": [
        "Preheat oven to 350 degrees F",
        "Gradually stir milk into Gravy Mix with whisk in medium saucepan",
        "Stirring frequently, cook on medium heat until gravy comes to boil",
        "Reduce heat and simmer 1 minute",
        "Stir in cream cheese, mayonnaise, mustard and ham",
        "Spread mixture in glass pie plate sprayed with no stick cooking spray",
        "Sprinkle with cheese",
        "Top with pickle chips, if desired",
        "Bake 20 minutes or until cheese is melted",
        "Serve with sliced baguette or crackers"

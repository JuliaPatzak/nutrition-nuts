    "ingredients": [
        "3 tablespoons vegetable oil",
        "1 cup self-rising cornmeal",
        "\u00be cup buttermilk",
        "1  egg",
        "1 tablespoon honey",
        "1 tablespoon self-rising cornmeal"
    ],
    "directions": [
        "Preheat oven to 400 degrees F (200 degrees C)",
        "In an 8 inch cast iron skillet, heat vegetable oil over low heat",
        "In a medium bowl, mix together cornmeal, buttermilk, egg and honey",
        "Carefully stir in the hot oil",
        "Sprinkle skillet with remaining 1 tablespoon corn meal",
        "Spoon mixture into skillet",
        "Bake in preheated oven for 22 minutes, or until firm",
        "Serve hot"

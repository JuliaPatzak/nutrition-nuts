    "ingredients": [
        "4 cups sauerkraut, drained",
        "1  onion, chopped",
        "1  apple - peeled, cored, and grated",
        "2  carrots, grated",
        "\u00bc cup canola oil",
        "3 tablespoons sugar",
        "1 teaspoon caraway seeds, or more to taste  (Optional)",
        "salt and ground black pepper to taste"
    ],
    "directions": [

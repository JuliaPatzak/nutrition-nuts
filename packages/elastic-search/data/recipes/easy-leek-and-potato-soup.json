    "ingredients": [
        "3 tablespoons butter",
        "1 small onion, chopped",
        "1 clove garlic, chopped",
        "3 cups chicken stock",
        "3  potatoes, peeled and diced",
        "3 large leeks, chopped",
        "\u00bd cup chopped fresh parsley, or to taste",
        "1 cube vegetable bouillon",
        "salt and freshly ground black pepper to taste"
    ],
    "directions": [
        "Melt butter in a large saucepan over medium heat",
        "Add onion and garlic",
        "Cook, stirring frequently, until slightly softened, about 3 minutes",
        "Add stock, potatoes, leeks, parsley, bouquet garni, bouillon cube, salt, and pepper",
        "Bring to a boil",
        "Reduce heat and simmer until potatoes are tender, about 30 minutes",
        "Remove soup from heat; discard bouquet garni",
        "Puree soup in the pot using a hand blender"
    ],

    "ingredients": [
        "1 tablespoon coconut oil",
        "1 cup sliced red onion",
        "1 large clove garlic, minced",
        "\u00bd teaspoon cumin seeds",
        "\u00bd head green cabbage, thinly sliced",
        "1 teaspoon salt",
        "1 teaspoon curry powder",
        "\u00bd teaspoon garam masala"
    ],
    "directions": [
        "Heat coconut oil over medium-high heat in a large nonstick pan or wok",
        "Add red onion and saute for 1 minute",
        "Stir in garlic and cumin seeds and stir-fry for 1 minute",
        "Add cabbage and toss until coated with oil",
        "Season with salt, curry powder, and garam masala",
        "Cook and stir until cabbage is tender, about 5 minutes"
    ],

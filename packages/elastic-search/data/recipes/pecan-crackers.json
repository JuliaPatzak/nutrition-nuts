    "ingredients": [
        "\u00bd (16 ounce) package graham crackers",
        "2 cups chopped pecans",
        "\u00bd cup white sugar",
        "\u00bd cup butter",
        "\u00bd cup margarine"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Line a baking sheet with graham crackers",
        "Sprinkle evenly with pecans",
        "In a small saucepan over medium heat, combine sugar, butter and margarine",
        "Bring to a boil and boil 3 minutes",
        "Remove from heat and spoon mixture over pecans, coating well",
        "Bake in preheated oven 12 minutes, until golden",
        "Cool completely before serving"
    ],

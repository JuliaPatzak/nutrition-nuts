    "ingredients": [
        "1 cup Karo\u00ae Light Corn Syrup",
        "1 cup sugar",
        "1 cup creamy peanut butter",
        "6 cups crispy rice cereal",
        "1 cup semi-sweet chocolate chips",
        "1 cup butterscotch chips"
    ],
    "directions": [
        "Cook corn syrup and sugar together in a large pan over medium heat, stirring to dissolve sugar",
        "Bring mixture to a boil",
        "Remove from heat",
        "Stir in peanut butter; mix well",
        "Add cereal; stir until evenly coated",
        "Pour into greased 13x9-inch pan and pat into place",
        "Melt chocolate and butterscotch chips together in saucepan over low heat, stirring constantly",
        "Spread over cereal",
        "Cool at least 45 minutes, or until firm",
        "Cut into bars"
    ],

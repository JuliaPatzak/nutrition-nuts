    "ingredients": [
        "\u00bc cup vegetable oil, divided",
        "1 pound halibut fillets",
        "\u00bd teaspoon salt",
        "\u00bc teaspoon ground black pepper",
        "\u00bc cup mayonnaise",
        "1  egg",
        "1 tablespoon mustard",
        "\u00bd teaspoon dried oregano",
        "\u00bd teaspoon dried thyme",
        "\u00bd teaspoon dried rosemary",
        "1 cup bread crumbs"
    ],
    "directions": [
        "Heat 2 tablespoons oil in a large skillet over medium-high heat",
        "Add halibut fillets; season with salt and pepper",
        "Cook until flesh flakes easily with a fork, about 4 minutes per side",
        "Transfer to a plate; cool until easily handled, about 5 minutes",
        "Flake into small chunks",
        "Whisk mayonnaise, egg, and mustard together in a large bowl",
        "Mix in oregano, thyme, and rosemary",
        "Stir in flaked halibut and bread crumbs",
        "Shape mixture into 6 cakes",
        "Heat remaining 2 tablespoons oil in the skillet over medium-high heat",
        "Add halibut cakes and cook until browned, about 5 minutes per side"
    ],

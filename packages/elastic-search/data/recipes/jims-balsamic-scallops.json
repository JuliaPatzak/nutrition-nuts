    "ingredients": [
        "2 pounds sea scallops",
        "\u00bd cup all-purpose flour",
        "4 teaspoons ground black pepper",
        "4 teaspoons garlic powder",
        "2 teaspoons paprika",
        "2 teaspoons dried marjoram",
        "2 teaspoons salt",
        "1 teaspoon cayenne pepper",
        "\u00bc cup olive oil",
        "\u00bd cup balsamic vinegar"
    ],
    "directions": [
        "Pat scallops dry with paper towels and set aside",
        "Place flour, black pepper, garlic powder, paprika, marjoram, salt, and cayenne pepper in a large plastic resealable bag and shake well to mix",
        "Add scallops to the bag and toss to coat with flour mixture",
        "Heat olive oil in a large skillet over medium heat",
        "Add all scallops in a single layer and sear until golden, 3 to 4 minutes per side",
        "Drizzle balsamic vinegar over seared scallops"
    ],

    "ingredients": [
        "\u00be cup white sugar",
        "2 tablespoons cornstarch",
        "4 cups chopped rhubarb",
        "1 tablespoon water",
        "1 tablespoon butter, diced",
        "1 teaspoon ground cinnamon",
        "1 cup all-purpose flour",
        "1 tablespoon white sugar",
        "1\u2009\u00bd teaspoons baking powder",
        "\u00bc teaspoon salt",
        "\u00bc cup butter",
        "\u00bc cup milk",
        "1  egg, beaten",
        "2 tablespoons white sugar"
    ],
    "directions": [
        "Preheat oven to 400 degrees F (200 degrees C)",
        "Lightly grease a 9 inch square baking dish",
        "In a saucepan, mix sugar and cornstarch for filling",
        "Stir in the rhubarb and water",
        "Bring to a boil",
        "Cook and stir for 1 minute",
        "Transfer to the prepared baking dish",
        "Dot with butter, and sprinkle with cinnamon",
        "In a medium bowl, sift together flour, sugar, baking powder, and salt for topping",
        "Cut in the butter until the mixture resembles coarse crumbs",
        "In a small bowl, mix together milk and egg",
        "Add all at once to dry ingredients, stirring just to moisten",
        "Drop by teaspoonfuls on top of the rhubarb mixture",
        "Sprinkle with sugar",
        "Bake for 20 minutes in the preheated oven, until crisp and lightly browned"
    ],

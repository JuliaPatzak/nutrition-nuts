    "ingredients": [
        "\u00bd cup butter",
        "\u2153 cup white sugar",
        "\u2153 cup brown sugar",
        "1  egg",
        "\u00bd teaspoon salt",
        "\u00bd teaspoon baking soda",
        "\u00bd teaspoon baking powder",
        "\u00bd teaspoon vanilla extract",
        "1\u2009\u00bc cups all-purpose flour",
        "\u00bd cup rolled oats",
        "1 cup jelly beans, cut in half"
    ],
    "directions": [
        "Preheat the oven to 325 degrees F (165 degrees C)",
        "Butter 2 cookie sheets",
        "Beat butter and sugars together using an electric mixer until creamy",
        "Beat in egg, salt, baking soda, baking powder, and vanilla extract",
        "Stir in flour and oats until just blended",
        "Fold in jelly beans",
        "Drop rounded spoonfuls of the cookie dough about 2 inches apart onto the prepared cookie sheets",
        "Bake in the preheated oven until edges are golden, 9 to 10 minutes"
    ],

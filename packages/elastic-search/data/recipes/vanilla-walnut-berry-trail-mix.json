    "ingredients": [
        "2 tablespoons butter",
        "2 teaspoons vanilla extract",
        "2 tablespoons white sugar",
        "2 cups California walnut halves and pieces",
        "1 cup yogurt-covered raisins",
        "\u00be cup raw pumpkin seeds",
        "2 cups freeze-dried strawberries"
    ],
    "directions": [
        "Melt butter in a medium skillet over medium heat",
        "Swirl in vanilla and sugar, then add walnuts",
        "Cook, stirring frequently, for 5 minutes, or until toasted",
        "Let cool completely",
        "Add raisins and pumpkin seeds to walnuts and toss well to mix",
        "Add strawberries and toss gently",
        "Store in an airtight container"
    ],

    "ingredients": [
        "1 (16 ounce) package spaghetti",
        "1 tablespoon olive oil",
        "\u00bc pound turkey bacon, cut into small pieces",
        "\u00bd large onion, chopped",
        "1 clove garlic, minced",
        "2 (15 ounce) cans tomato sauce",
        "1\u2009\u00bd teaspoons chopped fresh parsley",
        "\u00bc teaspoon dried basil",
        "1 teaspoon garlic powder",
        "\u00bd teaspoon pepper",
        "1 (15 ounce) can peas, drained",
        "\u00bc cup grated Romano cheese"
    ],
    "directions": [
        "Bring a large pot of lightly salted water to a boil",
        "Add pasta and cook for 8 to 10 minutes or until al dente; drain",
        "Heat oil in a large pot over medium heat",
        "Saute bacon, onion and garlic until lightly browned",
        "Stir in tomato sauce",
        "Season with parsley, basil, garlic powder and pepper",
        "Bring to a boil, reduce heat, and simmer 20 to 30 minutes, stirring occasionally",
        "Stir in peas",
        "Toss with pasta until evenly coated",
        "Sprinkle top with Romano"

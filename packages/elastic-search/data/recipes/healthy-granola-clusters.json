    "ingredients": [
        "\u00be cup raw sugar",
        "\u00bd cup butter",
        "1 teaspoon vanilla extract",
        "1\u2009\u00bc cups old-fashioned oats",
        "\u00be cup whole wheat flour",
        "\u00bc cup applesauce",
        "1  egg",
        "2 tablespoons molasses",
        "\u00bd teaspoon baking powder",
        "\u00bd teaspoon ground cinnamon",
        "\u00bc teaspoon ground nutmeg",
        "\u00bc teaspoon ground ginger",
        "\u00bc teaspoon ground cloves",
        "\u00bc teaspoon salt",
        "1 cup chopped walnuts",
        "\u00bd cup raisins"
    ],
    "directions": [
        "Preheat oven to 375 degrees F (190 degrees C)",
        "Beat sugar, butter, and vanilla extract together in a bowl using an electric mixer until smooth and creamy",
        "Add oats, whole wheat flour, applesauce, eggs, molasses, baking powder, cinnamon, nutmeg, ginger, cloves, and salt and mix well",
        "Fold walnuts and raisins into mixture; spoon onto a baking sheet, about 1 teaspoon per cluster",
        "Bake in the preheated oven until lightly browned and crisp, 10 to 13 minutes"
    ],

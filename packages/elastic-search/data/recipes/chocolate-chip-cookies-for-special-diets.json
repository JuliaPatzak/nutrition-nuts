    "ingredients": [
        "\u00bd cup butter, softened",
        "\u00be cup granulated artificial sweetener",
        "2 tablespoons water",
        "\u00bd teaspoon vanilla extract",
        "1  egg, beaten",
        "1\u2009\u215b cups all-purpose flour",
        "\u00bd teaspoon baking soda",
        "\u00bd teaspoon salt",
        "\u00bd cup semisweet chocolate chips",
        "\u00bd cup chopped pecans"
    ],
    "directions": [
        "Preheat oven to 375 degrees F (190 degrees C)",
        "In a medium bowl, cream together the butter and sugar substitute",
        "Mix in water, vanilla, and egg",
        "Sift together the flour, baking soda, and salt; stir into the creamed mixture",
        "Mix in the chocolate chips and pecans",
        "Drop cookies by heaping teaspoonfuls onto a cookie sheet",
        "Bake in the preheated oven for 10 to 12 minutes",
        "Remove from cookie sheets to cool on wire racks",
        "These cookies freeze well"
    ],

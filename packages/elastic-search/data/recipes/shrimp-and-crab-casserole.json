    "ingredients": [
        "2 cups water",
        "1 cup uncooked white rice",
        "1 pound crab meat",
        "2 (12 ounce) packages frozen shrimp, thawed",
        "2 cups mayonnaise",
        "1 cup chopped onion",
        "2 cups milk",
        "1 teaspoon hot pepper sauce",
        "\u00bd cup sliced almonds",
        "10  buttery round crackers, crushed"
    ],
    "directions": [
        "Preheat oven to 350 degrees F (175 degrees C)",
        "Lightly grease a 3 quart casserole dish",
        "In a saucepan, bring water to a boil",
        "Add rice and stir",
        "Reduce heat, cover and simmer for 20 minutes",
        "Combine crab meat, shrimp, rice, mayonnaise, onion, milk, hot sauce, and almonds in casserole dish",
        "Mix well",
        "Cover top of casserole with crumbled crackers",
        "Bake, uncovered, in preheated oven for 30 to 40 minutes, or until casserole is bubbling"
    ],

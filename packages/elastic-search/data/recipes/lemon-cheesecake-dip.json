    "ingredients": [
        "2 (8 ounce) packages cream cheese",
        "1 (8 ounce) container whipped topping",
        "1 (8 ounce) container sour cream",
        "1  lemon, zested",
        "1 tablespoon lemon juice",
        "1 tablespoon vanilla extract",
        "3 tablespoons confectioners' sugar"
    ],
    "directions": [
        "Bring all ingredients to room temperature",
        "Combine cream cheese, whipped topping, and sour cream in a bowl; beat with an electric mixer until well combined and creamy",
        "Mix in lemon zest, lemon juice, and vanilla extract until combined",
        "Add confectioners' sugar and mix thoroughly until smooth",
        "Cover and chill for 3 to 4 hours, or overnight"
    ],

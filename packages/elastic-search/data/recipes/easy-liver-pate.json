    "ingredients": [
        "1 pound braunschweiger liver sausage",
        "1 tablespoon milk",
        "1 tablespoon finely chopped onion",
        "\u00bd (8 ounce) package cream cheese, softened",
        "\u00bd teaspoon sugar",
        "\u00bd teaspoon minced garlic",
        "1 teaspoon chili powder",
        "1 tablespoon Worcestershire sauce"
    ],
    "directions": [
        "In a medium bowl, mix together the sausage, milk, onion, and cream cheese",
        "Season with sugar, garlic, chili powder and Worcestershire sauce, and mix until well blended",
        "Wrap in plastic wrap, and refrigerate for at least 2 hours, until firm"
    ],

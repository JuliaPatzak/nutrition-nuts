    "ingredients": [
        "6 cups peeled and chopped rhubarb",
        "2 cups strawberries, hulled and quartered",
        "\u00bd cup white sugar",
        "1 pinch salt",
        "1 cup half-and-half, or as needed"
    ],
    "directions": [
        "Combine rhubarb, strawberries, and sugar in a large bowl",
        "Mix well so that the sugar evenly coats the fruit",
        "Let stand for 15 minutes",
        "Add fruit and sugar mixture to a medium saucepan",
        "Cook over medium heat, stirring occasionally, for 15 minutes",
        "Season with salt",
        "Remove from heat and let cool",
        "Spoon small portions of the stewed fruit into bowls",
        "Drizzle each serving with a few tablespoons of half-and-half"
    ],

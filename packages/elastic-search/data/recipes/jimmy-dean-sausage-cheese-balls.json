    "ingredients": [
        "2 (16 ounce) packages Regular Flavor Jimmy Dean Pork Sausage",
        "1\u2009\u00bd cups all-purpose baking mix",
        "4 cups shredded sharp Cheddar cheese",
        "\u00bd cup finely chopped onion",
        "\u00bd cup finely chopped celery",
        "2 teaspoons black pepper",
        "2 teaspoons minced garlic  (Optional)"
    ],
    "directions": [
        "Preheat oven to 375 F",
        "Combine all ingredients in a large mixing bowl; stir well",
        "Form into 1inch balls",
        "Place on ungreased baking sheet; bake 18-20 minutes or until golden brown",
        "Remove from oven; cool 5 minutes before removing from pan",
        "Serve with cocktail forks or toothpicks"
    ],

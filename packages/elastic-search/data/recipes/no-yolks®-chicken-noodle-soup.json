    "ingredients": [
        "6 ounces NO YOLKS\u00ae Extra Broad Noodles",
        "1 tablespoon vegetable oil",
        "2 cloves garlic, chopped",
        "1  onion, chopped",
        "2  carrots, peeled and sliced",
        "2 ribs celery, sliced",
        "2  bay leaves",
        "2 sprigs fresh thyme",
        "1  rotisserie chicken, shredded, skin and bones removed*",
        "4 quarts chicken broth",
        "Salt and pepper"
    ],
    "directions": [
        "Heat oil in a large pot over medium heat",
        "Add garlic and onions and saute 2 minutes",
        "Add carrots, celery, bay leaves, thyme, chicken and broth",
        "Bring to a boil",
        "Add noodles",
        "Reduce heat to medium and simmer 10 minutes",
        "Remove bay leaves and thyme sprigs",
        "Season with salt and pepper to taste"
    ],

    "ingredients": [
        "2 cups milk",
        "1 package McCormick\u00ae Original Country Gravy Mix",
        "\u00bd pound ground breakfast sausage",
        "1 cup frozen mixed vegetables",
        "\u00bd cup shredded Cheddar cheese"
    ],
    "directions": [
        "Preheat oven to 350 degrees F",
        "Gradually stir milk into gravy mix with whisk in medium bowl",
        "Set aside",
        "Brown sausage in large skillet on medium heat, about 5 minutes",
        "Drain fat",
        "Return sausage to skillet",
        "Stir in gravy mixture and vegetables",
        "Bring to boil on medium heat, stirring frequently",
        "Reduce heat and simmer 2 minutes",
        "Spoon into 9-inch glass pie plate",
        "Sprinkle with cheese",
        "Bake 10 minutes or until cheese is melted",
        "Serve with potato tots"
    ],

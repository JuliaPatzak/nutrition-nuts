    "ingredients": [
        "4 large carrots, shredded",
        "1 large zucchini, shredded",
        "1 cup garbanzo bean flour",
        "2  eggs",
        "3 tablespoons mayonnaise",
        "1 tablespoon onion powder",
        "1 tablespoon garlic salt",
        "1 tablespoon red pepper flakes",
        "2 tablespoons vegetable oil, or as needed"
    ],
    "directions": [
        "Mix shredded carrots, shredded zucchini, flour, eggs, mayonnaise, onion powder, garlic salt, and red pepper flakes together in a bowl",
        "Heat oil in a skillet over medium-high heat",
        "Add batter by the tablespoon to the hot oil and fry like pancakes until both sides are cooked, about 5 minutes per batch"
    ],
